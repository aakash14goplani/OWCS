<!-- Record dependencies for the SiteEntry and the CSElement -->
<IF COND="IsVariable.seid=true"><THEN><RENDER.LOGDEP cid="Variables.seid" c="SiteEntry"/></THEN></IF>
<IF COND="IsVariable.eid=true"><THEN><RENDER.LOGDEP cid="Variables.eid" c="CSElement"/></THEN></IF>
<EXECSQL SQL="Select name from Publication where id = SessionVariables.pubid" LIST="QueryList" LIMIT="-1" TABLE="Publication" />
<IF COND="IsList.QueryList=true">
	<THEN>
      	<ICS.LISTGET LISTNAME="QueryList" FIELDNAME="name" /> 
   </THEN>
   <ELSE>
      The list does not exist.<br/>
   </ELSE>
</IF>
</FTCS>