<setvar NAME="result" VALUE="Variables.empty"/>
<!-- Add your code here -->
<!-- set the variable result to an error message for a possible error -->

<REPORTMANAGER.DEINSTALL/>
<IF COND="IsError.Variables.errno=true">
<THEN>
	<SETVAR NAME="result" VALUE="errno Variables.errno from REPORTMANAGER.DEINSTALL"/>
</THEN>
</IF>


<!-- End of user code -->
<!-- Check for error -->
<if COND="Variables.result!=Variables.empty">
    <then>
	    <logmsg STR="Xcelerate Installation Error in CommerceData/Deinstall: Variables.result"/>
	    ##ERROR## <csvar NAME="Variables.result"/><BR/>
    </then>
    <else>
	    ##SUCCESS## <csvar NAME="Variables.result"/><BR/>
    </else>
</if>

</FTCS>
