<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">

	<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
		<ARGUMENT NAME="AdHoc" VALUE="linkedCacheSummary"/>
		<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
	</CALLELEMENT>


	<REMOVEVAR NAME="OKActions"/>
	<REMOVEVAR NAME="Description"/>
	<REMOVEVAR NAME="ExecuteURL"/>
	
	<XLAT.LOOKUP KEY="fatwire/SystemTools/ManageCache/summary" ENCODE="false" VARNAME="_XLAT_"/>
	
	<SATELLITE.LINK ASSEMBLER="query" pagename="fatwire/systemtools/ManageCache" outstring="ExecuteURL">
		<satellite.argument name="page" value="linkedcache"/>
	</SATELLITE.LINK>
	
	<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
		<ARGUMENT NAME="Label" VALUE="Variables._XLAT_"/>
		<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
		<ARGUMENT NAME="ExecuteURL" VALUE="Variables.ExecuteURL"/>
		<ARGUMENT NAME="Image" VALUE="Variables.cs_imagedir/OMTree/TreeImages/tools.png"/>
	</CALLELEMENT>
	
	<REMOVEVAR NAME="Description"/>
	<REMOVEVAR NAME="OKActions"/>
	<REMOVEVAR NAME="_XLAT_"/>
	<REMOVEVAR NAME="ExecuteURL"/>
	
	<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
			<ARGUMENT NAME="AdHoc" VALUE="linkedCacheClusterInfo"/>
			<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
	</CALLELEMENT>
	
	<SATELLITE.LINK ASSEMBLER="query" pagename="fatwire/systemtools/ManageCache" outstring="ExecuteURL">
		<satellite.argument name="page" value="clusterInfo"/>
		<satellite.argument name="cachename" value="linked-cache"/>
	</SATELLITE.LINK>
	
	<XLAT.LOOKUP KEY="fatwire/SystemTools/ManageCache/clusterInfo" ENCODE="false" VARNAME="_XLAT_"/>
	<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
	<ARGUMENT NAME="Label" VALUE="Variables._XLAT_"/>
	<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
	<ARGUMENT NAME="ExecuteURL" VALUE="Variables.ExecuteURL"/>
	<ARGUMENT NAME="Image" VALUE="Variables.cs_imagedir/OMTree/TreeImages/tools.png"/>
	</CALLELEMENT>
</FTCS>

