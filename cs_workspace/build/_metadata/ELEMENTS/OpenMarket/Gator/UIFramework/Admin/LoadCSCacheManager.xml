<?xml version="1.0" ?>
<!DOCTYPE ftcs SYSTEM "futuretense_cs.dtd">
<ftcs version="1.2">
<!-- OpenMarket/Gator/UIFramework/Admin/LoadCSCacheManager
-
- INPUT
-
- OUTPUT
-
-->
<!-- Summary Link -->
	<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
		<ARGUMENT NAME="AdHoc" VALUE="cacheToolSummary"/>
		<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
	</CALLELEMENT>


	<REMOVEVAR NAME="OKActions"/>
	<REMOVEVAR NAME="Description"/>
	<REMOVEVAR NAME="ExecuteURL"/>
	
	<XLAT.LOOKUP KEY="fatwire/SystemTools/ManageCache/summary" ENCODE="false" VARNAME="_XLAT_"/>
	
	<SATELLITE.LINK ASSEMBLER="query" pagename="fatwire/systemtools/ManageCache" outstring="ExecuteURL">
		<satellite.argument name="page" value="summary"/>
		<satellite.argument name="cachename" value="cs-cache"/>
	</SATELLITE.LINK>
	
	<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
		<ARGUMENT NAME="Label" VALUE="Variables._XLAT_"/>
		<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
		<ARGUMENT NAME="ExecuteURL" VALUE="Variables.ExecuteURL"/>
		<ARGUMENT NAME="Image" VALUE="Variables.cs_imagedir/OMTree/TreeImages/tools.png"/>
	</CALLELEMENT>
	
<!-- Page Link -->
<IF COND="Variables.isPageCacheEnabled=true">
<THEN>
	<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
		<ARGUMENT NAME="AdHoc" VALUE="cacheToolPage"/>
		<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
	</CALLELEMENT>

	<REMOVEVAR NAME="Description"/>
	<REMOVEVAR NAME="OKActions"/>
	<REMOVEVAR NAME="_XLAT_"/>
	<REMOVEVAR NAME="ExecuteURL"/>
	
	<SATELLITE.LINK ASSEMBLER="query" pagename="fatwire/systemtools/ManageCache" outstring="ExecuteURL">
		<satellite.argument name="page" value="page"/>
		<satellite.argument name="cachename" value="cs-cache"/>
	</SATELLITE.LINK>

	<XLAT.LOOKUP KEY="fatwire/SystemTools/ManageCache/pageCache" ENCODE="false" VARNAME="_XLAT_"/>
	<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
		<ARGUMENT NAME="Label" VALUE="Variables._XLAT_"/>
		<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
		<ARGUMENT NAME="ExecuteURL" VALUE="Variables.ExecuteURL"/>
		<ARGUMENT NAME="Image" VALUE="Variables.cs_imagedir/OMTree/TreeImages/tools.png"/>
	</CALLELEMENT>
</THEN>
</IF>
	
<!-- Dependency Link -->

<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
		<ARGUMENT NAME="AdHoc" VALUE="cacheToolDependency"/>
		<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
</CALLELEMENT>

	<REMOVEVAR NAME="Description"/>
	<REMOVEVAR NAME="OKActions"/>
	<REMOVEVAR NAME="_XLAT_"/>
	<REMOVEVAR NAME="ExecuteURL"/>

	<SATELLITE.LINK ASSEMBLER="query" pagename="fatwire/systemtools/ManageCache" outstring="ExecuteURL">
		<satellite.argument name="page" value="dep"/>
		<satellite.argument name="cachename" value="cs-cache"/>
	</SATELLITE.LINK>
	<XLAT.LOOKUP KEY="fatwire/SystemTools/ManageCache/dependency" ENCODE="false" VARNAME="_XLAT_"/>

	<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
		<ARGUMENT NAME="Label" VALUE="Variables._XLAT_"/>
		<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
		<ARGUMENT NAME="ExecuteURL" VALUE="Variables.ExecuteURL"/>
		<ARGUMENT NAME="Image" VALUE="Variables.cs_imagedir/OMTree/TreeImages/tools.png"/>
	</CALLELEMENT>
<!-- Asset Cache -->
<IF COND="Variables.isAssetCacheEnabled=true">
<THEN>
	<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
			<ARGUMENT NAME="AdHoc" VALUE="assetCache"/>
			<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
	</CALLELEMENT>
	
	<REMOVEVAR NAME="Description"/>
	<REMOVEVAR NAME="OKActions"/>
	<REMOVEVAR NAME="_XLAT_"/>
	<REMOVEVAR NAME="ExecuteURL"/>

	<SATELLITE.LINK ASSEMBLER="query" pagename="fatwire/systemtools/ManageCache" outstring="ExecuteURL">
		<satellite.argument name="page" value="assetCache"/>
		<satellite.argument name="cachename" value="cs-cache"/>
	</SATELLITE.LINK>
	
	<XLAT.LOOKUP KEY="fatwire/SystemTools/ManageCache/assetCache" ENCODE="false" VARNAME="_XLAT_"/>

	<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
		<ARGUMENT NAME="Label" VALUE="Variables._XLAT_"/>
		<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
		<ARGUMENT NAME="ExecuteURL" VALUE="Variables.ExecuteURL"/>
		<ARGUMENT NAME="Image" VALUE="Variables.cs_imagedir/OMTree/TreeImages/tools.png"/>
	</CALLELEMENT>
</THEN>
</IF>

<!--  URL Cache -->
<IF COND="Variables.isURLCacheEnabled=true">
<THEN>
	<REMOVEVAR NAME="Description"/>
	<REMOVEVAR NAME="OKActions"/>
	<REMOVEVAR NAME="_XLAT_"/>
	<REMOVEVAR NAME="ExecuteURL"/>

	<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
			<ARGUMENT NAME="AdHoc" VALUE="urlCache"/>
			<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
	</CALLELEMENT>

	<SATELLITE.LINK ASSEMBLER="query" pagename="fatwire/systemtools/ManageCache" outstring="ExecuteURL">
		<satellite.argument name="page" value="urlCache"/>
		<satellite.argument name="cachename" value="cs-cache"/>
	</SATELLITE.LINK>

	<XLAT.LOOKUP KEY="fatwire/SystemTools/ManageCache/urlCache" ENCODE="false" VARNAME="_XLAT_"/>
	
	<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
		<ARGUMENT NAME="Label" VALUE="Variables._XLAT_"/>
		<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
		<ARGUMENT NAME="ExecuteURL" VALUE="Variables.ExecuteURL"/>
		<ARGUMENT NAME="Image" VALUE="Variables.cs_imagedir/OMTree/TreeImages/tools.png"/>
	</CALLELEMENT>
</THEN>
</IF>

<!-- Cluster Info -->
	<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
		<ARGUMENT NAME="AdHoc" VALUE="cacheToolClusterInfo"/>
		<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
	</CALLELEMENT>

	<REMOVEVAR NAME="ExecuteURL"/>
	<REMOVEVAR NAME="Description"/>
	<REMOVEVAR NAME="OKActions"/>
	<REMOVEVAR NAME="_XLAT_"/>
	
	<XLAT.LOOKUP KEY="fatwire/SystemTools/ManageCache/clusterInfo" ENCODE="false" VARNAME="_XLAT_"/>
	
	<SATELLITE.LINK ASSEMBLER="query" pagename="fatwire/systemtools/ManageCache" outstring="ExecuteURL">
		<satellite.argument name="page" value="clusterInfo"/>
		<satellite.argument name="cachename" value="cs-cache"/>
	</SATELLITE.LINK>

	<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
		<ARGUMENT NAME="Label" VALUE="Variables._XLAT_"/>
		<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
		<ARGUMENT NAME="ExecuteURL" VALUE="Variables.ExecuteURL"/>
		<ARGUMENT NAME="Image" VALUE="Variables.cs_imagedir/OMTree/TreeImages/tools.png"/>
	</CALLELEMENT>

</ftcs>