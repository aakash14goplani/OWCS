<!--This element contains the code for the Menu Items in  , System Tools -> System Information -->

<!-- CS Information Display link -->

<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
	<ARGUMENT NAME="AdHoc" VALUE="CSInfo"/>
	<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
</CALLELEMENT>

<REMOVEVAR NAME="ExecuteURL"/>
<REMOVEVAR NAME="Description"/>

<XLAT.LOOKUP KEY="dvin/TreeApplet/SystemTools/SystemInformation/CSInfo/CSInfo" ENCODE="false" VARNAME="_XLAT_"/>

<SATELLITE.LINK ASSEMBLER="query" pagename="fatwire/systemtools/SystemInfo/CSInfo" outstring="ExecuteURL">
</SATELLITE.LINK>

<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
	<ARGUMENT NAME="Label" VALUE="Variables._XLAT_"/>
	<ARGUMENT NAME="Description" VALUE="Variables._XLAT_"/>
	<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
	<ARGUMENT NAME="ExecuteURL" VALUE="Variables.ExecuteURL"/>
	<ARGUMENT NAME="Image" VALUE="Variables.cs_imagedir/OMTree/TreeImages/tools.png"/>
</CALLELEMENT>

<!-- DBInfo -->

<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
	<ARGUMENT NAME="AdHoc" VALUE="DBInfo"/>
	<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
</CALLELEMENT>

<REMOVEVAR NAME="ExecuteURL"/>
<REMOVEVAR NAME="Description"/>

<XLAT.LOOKUP KEY="dvin/TreeApplet/SystemTools/SystemInformation/DBInfo/DBInfo" ENCODE="false" VARNAME="_XLAT_"/>

<SATELLITE.LINK ASSEMBLER="query" pagename="fatwire/systemtools/SystemInfo/DBInfo" outstring="ExecuteURL">
</SATELLITE.LINK>

<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
	<ARGUMENT NAME="Label" VALUE="Variables._XLAT_"/>
	<ARGUMENT NAME="Description" VALUE="Variables._XLAT_"/>
	<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
	<ARGUMENT NAME="ExecuteURL" VALUE="Variables.ExecuteURL"/>
	<ARGUMENT NAME="Image" VALUE="Variables.cs_imagedir/OMTree/TreeImages/tools.png"/>
</CALLELEMENT>


<!-- Thread dump -->

<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
	<ARGUMENT NAME="AdHoc" VALUE="threadDump"/>
	<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
</CALLELEMENT>

<REMOVEVAR NAME="ExecuteURL"/>
<REMOVEVAR NAME="Description"/>

<XLAT.LOOKUP KEY="dvin/TreeApplet/SystemTools/SystemInformation/ThreadDump/thread" ENCODE="false" VARNAME="_XLAT_"/>

<SATELLITE.LINK ASSEMBLER="query" pagename="fatwire/systemtools/SystemInfo/ThreadInfo" outstring="ExecuteURL">
</SATELLITE.LINK>

<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
	<ARGUMENT NAME="Label" VALUE="Variables._XLAT_"/>
	<ARGUMENT NAME="Description" VALUE="Variables._XLAT_"/>
	<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
	<ARGUMENT NAME="ExecuteURL" VALUE="Variables.ExecuteURL"/>
	<ARGUMENT NAME="Image" VALUE="Variables.cs_imagedir/OMTree/TreeImages/tools.png"/>
</CALLELEMENT>

</ftcs>