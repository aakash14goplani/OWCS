<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Gator/AttributeTypes/DisplayTEXTFIELD
--
-- INPUT
--
-- OUTPUT
--
-->

<setvar NAME="blank" VALUE="Variables.empty"/>
<presentation.getblanked NAME="Variables.PresInst" VARNAME="blank"/>
<IF COND="Variables.EditingStyle=single">
<THEN>
	<IF COND="Variables.blank=true">
	<THEN>
		<setvar NAME="doDefaultDisplay" VALUE="no"/>
		
		<setvar NAME="MyAttrVal" VALUE="Variables.empty"/>
		<setvar NAME="firsttime" VALUE="true"/>
		<if COND="IsList.AttrValueList=true">
		<then>
			<IF COND="AttrValueList.#numRows!=0">
			<THEN>
				<loop LIST="AttrValueList">
					<if COND="Variables.firsttime=true">
					<then>
							<setvar NAME="firsttime"  VALUE="false"/>
							<IF COND="Variables.blank=true">
							<THEN>
								<IF COND="AttrValueList.value!=Variables.empty">
								<THEN>
									<TEXTFORMAT.LENGTH VALUE="AttrValueList.value" VARNAME="valueLength"/>
									<LOOP COUNT="Variables.valueLength">
										<setvar NAME="MyAttrVal" VALUE="Variables.MyAttrVal*"/>
									</LOOP>
								</THEN>
								</IF>
							</THEN>
							<ELSE>
								<setvar NAME="MyAttrVal"  VALUE="AttrValueList.value"/>
							</ELSE>
							</IF>
					</then>
					<else>
						   <!-- <setvar NAME="MyAttrVal"  VALUE="Variables.MyAttrVal , AttrValueList.value"/>-->
							<setvar NAME="MyAttrVal"  VALUE="Variables.MyAttrVal , "/>
							<IF COND="Variables.blank=true">
								<THEN>
									<IF COND="AttrValueList.value!=Variables.empty">
									<THEN>
										<TEXTFORMAT.LENGTH VALUE="AttrValueList.value" VARNAME="valueLength"/>
										<LOOP COUNT="Variables.valueLength">
											<setvar NAME="MyAttrVal" VALUE="Variables.MyAttrVal*"/>
										</LOOP>
									</THEN>
									</IF>
								</THEN>
								<ELSE>
									<setvar NAME="MyAttrVal"  VALUE="AttrValueList.value"/>
								</ELSE>
								</IF>
					</else>
					</if>
				</loop>
			</THEN>
			</IF>
		</then>
		</if>
		<if COND="Variables.SpacerBar=1">
		<then>
			<!--<callelement NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacerBar"/>-->
			<setvar NAME="SpacerBar" VALUE="0"/>
		</then>
		<else>
			
		</else>
		</if>
		<callelement NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
		<tr>
			<callelement NAME="OpenMarket/Gator/FlexibleAssets/Common/DisplayAttributeName"/>
			<td></td>
			<td><STRING.STREAM VALUE="Variables.MyAttrVal"/></td>
		</tr>
	</THEN>
	</IF>
</THEN>
</IF>
</FTCS> 