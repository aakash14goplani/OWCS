<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Gator/FlexibleAssets/FlexTmpls/GetTemplateType
--
-- INPUT - none
--
-- OUTPUT - TemplateType
--
-->

<!-- are we working with a group template or asset template? -->
<setvar NAME="TemplateType" VALUE="group"/>
<ATM.GETASSETS FILTER="com.openmarket.gator.flextemplates.FlexTemplateManager" LISTVARNAME="out"/>
<LOOP LIST="out">
	<IF COND="Variables.AssetType=out.assettype">
	<THEN>
		<setvar NAME="TemplateType" VALUE="asset"/>
	</THEN>
	</IF>
</LOOP>

</FTCS> 
