<setvar NAME="addSQLError" VALUE="Variables.empty"/>
<IF COND="IsVariable.sqltype=true">
<THEN>
	<MISC.GENERATESYSTEMSQL ASSETTYPE="Variables.AssetType" STUBTYPE="Variables.sqltype"/>
	<IF COND="Variables.errno!=0">
	<THEN>
        <setvar NAME="addSQLError" VALUE="Variables.errno, Variables.errdetail1"/>
		<!-- <csvar NAME="Adding to SystemSQL table failed with Variables.errno, Variables.errdetail1!"/><br/>  -->
	</THEN>
	</IF>
</THEN>
</IF>

</FTCS> 
