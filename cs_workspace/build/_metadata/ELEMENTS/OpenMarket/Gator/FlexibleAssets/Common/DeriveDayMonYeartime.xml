<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Gator/FlexibleAssets/Common/DeriveDayMonYeartime
--
-- INPUT
--
-- OUTPUT
--
-->

	 <setvar NAME="tempval" VALUE="AttrValueList.value"/>
 		<SUBSTRING STR="AttrValueList.value"
		 OUTSTR="yearstr"
		INDEX="0"
		ENDINDEX="4"
		/>
		
		<SUBSTRING STR="AttrValueList.value"
		 OUTSTR="monstr"
		INDEX="5"
		ENDINDEX="7"
		/>
		
		<SUBSTRING STR="AttrValueList.value"
		 OUTSTR="daystr"
		INDEX="8"
		ENDINDEX="10"
		/>
		
		<SUBSTRING STR="AttrValueList.value"
		 OUTSTR="hourstr"
		INDEX="11"
		ENDINDEX="13"
		/>
		
		<SUBSTRING STR="AttrValueList.value"
		 OUTSTR="minstr"
		INDEX="14"
		ENDINDEX="16"
		/>
		
		<SUBSTRING STR="AttrValueList.value"
		 OUTSTR="secstr"
		INDEX="17"
		ENDINDEX="19"
		/>


 

</FTCS> 
