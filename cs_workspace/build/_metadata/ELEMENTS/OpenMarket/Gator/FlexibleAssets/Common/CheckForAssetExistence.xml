<!--
	Note: 
-->

<ASSET.GET NAME="theCurrentAsset" FIELD="name" OUTPUT="checkname"/>
<ASSET.LIST TYPE="Variables.AssetType" LIST="duplicateList" FIELD1="name" VALUE1="Variables.checkname" PUBID="SessionVariables.pubid" EXCLUDEVOIDED="true"/>
<IF COND="duplicateList.#numRows!=0">
<THEN>	
	<SETVAR name="errno" value="-12075" />
</THEN>
<ELSE>
    <SETVAR NAME="errno" VALUE="0"/>
</ELSE>
</IF>

</FTCS>