<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Gator/Util/HasMarketStudio
--
-- INPUT
--
-- OUTPUT
--	hasMarketStudio true or false
--  errno not zero if no ms.ini or related problems
-->

<setvar NAME="errno" VALUE="0"/>
<setvar NAME="hasMarketStudio" VALUE="false"/>

<!-- <LOADPROPERTY FILE="Variables.inifile;ms.ini"/>	-->
<PROPERTY.GET INIFILE="ms.ini" PARAM="ms.enable" VARNAME="LocalLoadMarket"/>
<if COND="Variables.errno!=0">
	<then>
		<br/><XLAT.STREAM KEY="dvin/Util/FlexAssets/LoadMSError" errno="Variables.errno" EVALALL="false"/>  
	</then>
	<else>
		<if COND="Variables.LocalLoadMarket=1">
			<then>
				<setvar NAME="hasMarketStudio" VALUE="true"/>
			</then>
		</if>
		<REMOVEVAR NAME="LocalLoadMarket"/>
	</else>
</if>

</FTCS> 
