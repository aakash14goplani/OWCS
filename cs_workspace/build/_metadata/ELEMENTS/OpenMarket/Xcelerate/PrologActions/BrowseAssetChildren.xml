<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Xcelerate/PrologActions/BrowseAssetChildren
-
- INPUT
-
- OUTPUT
-
-->

<IF COND="Variables.c=Page">
<THEN>
<IF COND="IsVariable.p=false">
<THEN>
		<ASSET.LOAD NAME="currPage" TYPE="Page" OBJECTID="Variables.cid"/>

<setvar NAME="errno" VALUE="0"/>
<SETVAR NAME="p" VALUE="Variables.empty"/>
<LOOP UNTIL="Variables.errno!=0">
<ASSET.SITEPARENT NAME="currPage" OUTNAME="currPage"/>
	<IF COND="Variables.errno=0">
	<THEN>
		<ASSET.GET NAME="currPage" FIELD="id" OUTPUT="ParentId"/>
		<setvar NAME="p" VALUE="Variables.ParentId,Variables.p"/>
	</THEN>
	</IF>
</LOOP>
</THEN>
</IF>

</THEN>
</IF>
</FTCS> 
