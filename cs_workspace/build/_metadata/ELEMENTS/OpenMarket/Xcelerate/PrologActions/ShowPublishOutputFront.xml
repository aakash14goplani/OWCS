<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Xcelerate/PrologActions/ShowPublishOutputFront
-
- INPUT
-
- OUTPUT
-
-->
<!-- Determines the target from publish session -->
<STRING.ENCODE VARIABLE="id" VARNAME="OutputQString"/>
<PUBSESSIONMANAGER.LOAD ID="Variables.OutputQString" OBJVARNAME="pubsess"/>
<PUBSESSION.GETTARGET NAME="pubsess" VARNAME="pubsess:target"/>
<PUBTARGETMANAGER.LOAD ID="Variables.pubsess:target" OBJVARNAME="pubtgt"/>

<!-- Verifies if user has permission to execute this element -->
<CALLELEMENT NAME="OpenMarket/Xcelerate/Actions/Security/PublishAccessCheck">
    <ARGUMENT NAME="targetid" VALUE="Variables.pubsess:target"/>
</CALLELEMENT>
</FTCS> 
