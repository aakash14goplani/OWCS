<?XML VERSION="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Xcelerate/PrologActions/Publish/Mirror1/AssetTypeRemoteElement
-
-- INPUT
--	sessionid	- pub session id
-- pubid	- optional, if provided asset types will be enabled in the publication
-- OUTPUT
--
-->

<IF COND="IsVariable.pubid=true">
<THEN>
	<ASSETTYPE.SavePublishedAssetTypes SESSIONID="Variables.sessionid" PUBID="Variables.pubid"/>
</THEN>
<ELSE>
	<ASSETTYPE.SavePublishedAssetTypes SESSIONID="Variables.sessionid" />
</ELSE>
</IF>
<if COND="IsError.Variables.errno=true">
<then>
	<setvar NAME="saveerrno" VALUE="Variables.errno"/>
	<XLAT.LOOKUP VARNAME="result" KEY="dvin/UI/Error/assettype.savepublishedassettypedetail1" errno="Variables.errno" sessionid="Variables.sessionid" errdetail1="Variables.errdetail1" EVALALL="false"/>
	<setvar NAME="errno" VALUE="Variables.saveerrno"/>
	<logmsg STR="Error from ASSETTYPE.SavePublishedAssetType session Variables.sessionid Error Variables.errno (Variables.errdetail1)"/>
</then>
</if>



</FTCS>
