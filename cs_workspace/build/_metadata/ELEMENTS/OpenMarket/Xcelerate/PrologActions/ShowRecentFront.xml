<IF COND="IsVariable.historyarray!=true">
<THEN>
	<SETVAR NAME="historyarray" VALUE="Variables.empty"/>
</THEN>
<ELSE>
<IF COND="Variables.historyarray!=Variables.empty">
<THEN>
	<STRINGLIST STR="Variables.historyarray" DELIM="," NAME="historylist"/>
	<STRINGLIST STR="Variables.typearray" DELIM="," NAME="typelist"/>
	<SETVAR NAME="HistoryLength" VALUE="0"/>
	<SETVAR NAME="HistoryLength" VALUE="historylist.#numRows"/>
	<LISTOBJECT.CREATE NAME="histobj" COLUMNS="assetid,assettype"/>
	<LOOP COUNT="Variables.HistoryLength">
		<LISTOBJECT.ADDROW NAME="histobj" assetid="historylist.ITEM" assettype="typelist.ITEM"/>
		<SETROW LIST="historylist" ACTION="NEXT"/>
		<SETROW LIST="typelist" ACTION="NEXT"/>
	</LOOP>
	<LISTOBJECT.TOLIST NAME="histobj" LISTVARNAME="theAssets"/>
	
</THEN>
</IF>
</ELSE>
</IF>

</FTCS> 
