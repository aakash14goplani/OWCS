	<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/BasicEnvironment"/>

	<HTML>
	<HEAD>

		<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/StandardHeader"/>
	

		<SATELLITE.LINK ASSEMBLER="query" pagename="OpenMarket/Xcelerate/UIFramework/LoginPost" outstring="loginPostURL">
		</SATELLITE.LINK>

		<TITLE>Content Server Login </TITLE>


	</HEAD>
	
<USERMANAGER.LOGINUSER USERNAMEVARIABLE="username" PASSWORDVARIABLE="password" VARNAME="loggedIn"/>
<IF COND="Variables.loggedIn=true">
<THEN>

	<SCRIPT LANGUAGE="JavaScript">
	document.location.replace('<CSVAR NAME="Variables.loginPostURL"/>');
	</SCRIPT> 

</THEN>
<ELSE>
    <SATELLITE.LINK ASSEMBLER="query" pagename="OpenMarket/Xcelerate/UIFramework/ShowBanner" outstring="urlshowbanner">
    </SATELLITE.LINK>
    <SATELLITE.LINK ASSEMBLER="query" pagename="OpenMarket/Xcelerate/UIFramework/LoginError" outstring="urlloginerror">
     </SATELLITE.LINK>
     <FRAMESET ROWS="35, *" FRAMEBORDER="0" BORDER="0" FRAMESPACING="0">
        <FRAME NAME="XcelBanner" SRC="Variables.urlshowbanner" MARGINWIDTH="0" MARGINHEIGHT="0" FRAMEBORDER="0" SCROLLING="no" REPLACEALL="Variables.urlshowbanner"/>
		<FRAME NAME="LoginError" SRC="Variables.urlloginerror" MARGINWIDTH="0" FRAMEBORDER="0" REPLACEALL="Variables.urlloginerror"/>
	</FRAMESET>
</ELSE>
</IF>
</HTML>
