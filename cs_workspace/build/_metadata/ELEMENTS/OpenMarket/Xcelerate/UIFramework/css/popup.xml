<style>
/* Styles for PickAssetPopup window */
body#OpenMarket-Xcelerate-Actions-PickAssetPopup {
margin : 0;
padding : 0;
font-size : 11px;
}
#pappanel {
    clear: both; /* Gecko fix */
    padding: 15 15 40 15px;
    background-color:#f6f8fa;
	border-width:1px;
	border-color:#CCC;
	border-style: solid;

}
    #papheader {
      float:left;
      width:100%;
//      font-size:93%;
      line-height:normal;
      background:white;
	  border:none;
      }

    #papheader .title-text
    { padding : 4px 4px 0 10px; }

    #papheader ul {
      margin:0;
	  padding:10px 10px 0px 10px;
	  list-style:none;
      }
    #papheader li{
         display:inline;
         float:left;
         background:url(<CSVAR NAME="Variables.cs_imagedir"/>/graphics/common/icon/OffTabLeft.gif) no-repeat left top;
         padding:0 5 0 2px;
         margin:0;
      }

   #papheader a span{
      float:left;
      display:block;
	  background:url(<CSVAR NAME="Variables.cs_imagedir"/>/graphics/common/icon/OffTabRight.gif) no-repeat right top;
	  padding:6 2 4 3px;
	  color:#686868;
	  font-size:8pt;

    }
    #papheader a{
          outline: none;
          float:left;
          display:block;
          background:url(<CSVAR NAME="Variables.cs_imagedir"/>/graphics/common/icon/OffTabSliver.gif) repeat top left;
          text-decoration:none;
          text-align:center;
	      color:#000;
      }

    #papheader li:hover {
        background:url(<CSVAR NAME="Variables.cs_imagedir"/>/graphics/common/icon/OffTabLeft.gif) no-repeat left top;
        color:#686868;

      }
    #papheader li:hover a {
      background:url(<CSVAR NAME="Variables.cs_imagedir"/>/graphics/common/icon/OnTabSliver.gif) repeat top left;
        color:#686868;
      }
    #papheader li:hover a span{
      background:url(<CSVAR NAME="Variables.cs_imagedir"/>/graphics/common/icon/OffTabRight.gif) no-repeat right top;
        color:#686868;
      }
    #papheader li.sfhover {
    	background:url(<CSVAR NAME="Variables.cs_imagedir"/>/graphics/common/icon/OffTabLeft.gif) no-repeat top left;
        color:#686868;
    	}
    #papheader li.sfhover a {
    	background:url(<CSVAR NAME="Variables.cs_imagedir"/>/graphics/common/icon/OnTabSliver.gif) repeat left top;
        color:#686868;
    	}

    #papheader li.sfhover a span{
    	background:url(<CSVAR NAME="Variables.cs_imagedir"/>/graphics/common/icon/OffTabRight.gif) no-repeat right top;
        color:#686868;
    	}

    #papheader #current li{
        float:left;
	  display:block;
	  background:url(<CSVAR NAME="Variables.cs_imagedir"/>/graphics/common/icon/OnTabLeft.gif) no-repeat right top;
    padding:0 0 0 0px;
      color:#686868;
	  font-weight:bold;
	  font-size:8pt;
      background-position:100% -100px;
      }
    #papheader #current a {
     float:left;
      display:block;
	  background:url(<CSVAR NAME="Variables.cs_imagedir"/>/graphics/common/icon/OnTabSliver.gif) repeat top;
    color:#686868;
	  font-weight:bold;
	  font-size:8pt;
      background-position:100% -100px;
      }

    #papheader #current a span{
     loat:left;
	  display:block;
	  background:url(<CSVAR NAME="Variables.cs_imagedir"/>/graphics/common/icon/OnTabRight.gif) no-repeat right top;
	  padding:6 2 5 3px;
	  color:#686868;
	  font-weight:bold;
	  font-size:8pt;

    }
/* End styles for PickAssetPopup window */

