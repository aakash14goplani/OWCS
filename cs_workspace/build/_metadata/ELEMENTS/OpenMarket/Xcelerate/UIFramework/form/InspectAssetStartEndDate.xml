<?xml version="1.0" ?>
<!DOCTYPE ftcs SYSTEM "futuretense_cs.dtd">
<ftcs version="1.2">
<!-- OpenMarket/Xcelerate/UIFramework/form/InspectAssetStartEndDate
-
- INPUT
-
- OUTPUT
-
-->
<ICS.GETPROPERTY NAME="cs.sitepreview" FILE="futuretense.ini" OUTPUT="enableSitePreview"/>
<IF COND="Variables.enableSitePreview!=disabled">
<THEN>
	<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
	<tr>
		<td class="form-label-text">
			<XLAT.STREAM KEY="fatwire/Alloy/UI/StartDate"/>: 
		</td>
		<td></td>
		<td class="form-inset">
			<DATEFORMAT.GETDATETIME NAME="_FormatDate_" VALUE="Variables.ContentDetails:startdate" VALUETYPE="jdbcdate" VARNAME="ContentDetails:startdate"/>
			<STRING.LENGTH VALUE="Variables.ContentDetails:startdate" VARNAME="sdateLength"/> 
			<IF COND="Variables.sdateLength=0">
			<THEN>
				<span class="disabledText"><XLAT.STREAM KEY="UI/Forms/NotAvailable"/></span>
			</THEN>
			<ELSE>
				<STRING.STREAM VARIABLE="ContentDetails:startdate"/>
			</ELSE>
			</IF>
		</td>
	</tr>
	<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>					
	<tr>
		<td class="form-label-text">
			<XLAT.STREAM KEY="fatwire/Alloy/UI/EndDate"/>: 
		</td>
		<td></td>
		<td class="form-inset">
			<DATEFORMAT.GETDATETIME NAME="_FormatDate_" VALUE="Variables.ContentDetails:enddate" VALUETYPE="jdbcdate" VARNAME="ContentDetails:enddate"/>
			<STRING.LENGTH VALUE="Variables.ContentDetails:enddate" VARNAME="edateLength"/> 
			<IF COND="Variables.edateLength=0">
			<THEN>
				<span class="disabledText"><XLAT.STREAM KEY="UI/Forms/NotAvailable"/></span>
			</THEN>
			<ELSE>
				<STRING.STREAM VARIABLE="ContentDetails:enddate"/>
			</ELSE>
			</IF>
		</td>					
	</tr>
</THEN>
</IF>
</ftcs>