<table width="100%" cellPadding="0" cellSpacing="0" style="background-image:url(Variables.cs_imagedir/graphics/common/toolbar/topnav_bg.gif);" REPLACEALL="Variables.cs_imagedir">
 	<tr>
      	<td align="left" valign="top" width="4">
      		<!--<img src="Variables.cs_imagedir/graphics/common/toolbar/topnav_bg_left.gif" REPLACEALL="Variables.cs_imagedir"/>-->
      	</td>
      	<td align="left" valign="top">
		  	<table width="100%" cellPadding="0" cellSpacing="0" border="0">
		  	  <tr>
		  	    <td align="left">
		  	      <img src="Variables.cs_imagedir/graphics/common/toolbar/fw_logo.gif" REPLACEALL="Variables.cs_imagedir"/>
		  	    </td>
		  	    <td align="right" valign="middle" class="toprowwhite">
					<img src="Variables.cs_imagedir/graphics/common/toolbar/spacer.gif" width="5" height="1" REPLACEALL="Variables.cs_imagedir"/>
                      	<IF COND="Variables.enableAdminTab=true">
                              <THEN>
                                  <XLAT.LOOKUP KEY="dvin/Common/ShowLeftHandNavigation" VARNAME="_XLAT_" ESCAPE="true"/>
					   	          <span id="controlToggleDiv" ><img id="controlToggle" src="Variables.cs_imagedir/graphics/common/toolbar/togglecontroloff.gif" alt="Variables._XLAT_" title="Variables._XLAT_" onclick="switchToggleIcon('controlToggleDiv','FullWindowToggleDiv');" style="vertical-align:middle;  cursor:pointer; cursor:hand" REPLACEALL="Variables.cs_imagedir,Variables._XLAT_"/></span>
                                  <XLAT.LOOKUP KEY="dvin/Common/HideLeftHandNavigation" VARNAME="_XLAT_" ESCAPE="true"/>
                 				    <span id="FullWindowToggleDiv" style="display:none;"><img id="fullWindowToggle" src="Variables.cs_imagedir/graphics/common/toolbar/togglewindowfulloff.gif" alt="Variables._XLAT_" title="Variables._XLAT_" onclick="switchToggleIcon('FullWindowToggleDiv','controlToggleDiv');"	style="vertical-align:middle; cursor:pointer; cursor:hand" REPLACEALL="Variables.cs_imagedir,Variables._XLAT_"/></span>
                              </THEN>
                              <ELSE>
                                  <XLAT.LOOKUP KEY="dvin/Common/ShowLeftHandNavigation" VARNAME="_XLAT_" ESCAPE="true"/>
					   	          <span id="controlToggleDiv" style="display:none;"><img id="controlToggle" src="Variables.cs_imagedir/graphics/common/toolbar/togglecontroloff.gif" alt="Variables._XLAT_" title="Variables._XLAT_" onclick="switchToggleIcon('controlToggleDiv','FullWindowToggleDiv');" style="vertical-align:middle;  cursor:pointer; cursor:hand" REPLACEALL="Variables.cs_imagedir,Variables._XLAT_"/></span>
                                  <XLAT.LOOKUP KEY="dvin/Common/HideLeftHandNavigation" VARNAME="_XLAT_" ESCAPE="true"/>
                 				    <span id="FullWindowToggleDiv" ><img id="fullWindowToggle" src="Variables.cs_imagedir/graphics/common/toolbar/togglewindowfulloff.gif" alt="Variables._XLAT_" title="Variables._XLAT_" onclick="switchToggleIcon('FullWindowToggleDiv','controlToggleDiv');"	style="vertical-align:middle; cursor:pointer; cursor:hand" REPLACEALL="Variables.cs_imagedir,Variables._XLAT_"/></span>
                              </ELSE>
                          </IF>

					 <img src="Variables.cs_imagedir/graphics/common/toolbar/spacer.gif" width="20" height="1" REPLACEALL="Variables.cs_imagedir"/>


      				  <XLAT.STREAM KEY="dvin/Common/User"/>:  <A HREF="javascript:void(0);"  ONCLICK="javascript:parent.frames['XcelWorkFrames'].frames['XcelAction'].location.href='Variables.modifyURL';" REPLACEALL="Variables.modifyURL" >
      				   <USERMANAGER.GETDISPLAYABLELOGINUSERNAME VARNAME="loginusername"/><STRING.STREAM VARIABLE="loginusername"/></A> | <a href="javascript:void(0);" onclick="window.open('CS.Property.cs.documentation','help_window')" REPLACEALL="CS.Property.cs.documentation,SessionVariables.locale" ><XLAT.STREAM KEY="dvin/UI/Help"/></a> | <a HREF="Variables.urlLogoutFront" REPLACEALL="Variables.urlLogoutFront"><XLAT.STREAM KEY="dvin/Common/Logout"/></a>

					<img src="Variables.cs_imagedir/graphics/common/toolbar/spacer.gif" width="12" height="1" REPLACEALL="Variables.cs_imagedir"/>
		  	    </td>
		  	  </tr>
		  	</table>
		  	<table width="100%" cellPadding="0" cellSpacing="0" border="0">
		  	  <tr>
		  	    <td align="left" width="50%">
				<IF COND="IsElement.OpenMarket/Xcelerate/UIFramework/Live/LoadLiveToolbar=true">
					<THEN>
					 <CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Live/LoadLiveToolbar"/>
				   </THEN>
					<ELSE>
					<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/LoadToolBar"/> 
					</ELSE>
				</IF>

				</td>

		  	    <td align="right" width="50%" class="middlerowregular">
					<img src="Variables.cs_imagedir/graphics/common/toolbar/spacer.gif" width="10" height="1" REPLACEALL="Variables.cs_imagedir"/>

					</td>
		  	  </tr>
		  	</table>


  		</td>
      	<td align="right" valign="top" width="4">
      		<!--<img src="Variables.cs_imagedir/graphics/common/toolbar/topnav_bg_right.gif" REPLACEALL="Variables.cs_imagedir"/>-->
      	</td>

	</tr>
</table>


</ftcs>