<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Xcelerate/Errors/AddAssetChildError
--
-- INPUT
--
-- OUTPUT
--
-->
<SETVAR NAME="saveerrno" VALUE="Variables.errno"/>
<!-- Load the information about the asset type -->

	<ASSETTYPE.LOAD NAME="type" TYPE="Variables.AssetType"/>
	<ASSETTYPE.SCATTER NAME="type" PREFIX="AssetTypeObj"/>
	<ASSET.LOAD NAME="badChild" TYPE="Variables.childtype" OBJECTID="Variables.childid"/>
	<ASSET.GET NAME="badChild" FIELD="name" OUTPUT="badChild:name"/>
	<ASSET.GETASSETTYPE NAME="badChild" OUTNAME="childat"/>
	<ASSETTYPE.GET NAME="childat" FIELD="description" OUTPUT="childtypedescription"/>

<!-- Display the error -->

	<XLAT.STREAM KEY="dvin/UI/Error/AddAssetChildErrorHeader" ESCAPE="false" ENCODE="false"/>
	<P>
		<XLAT.STREAM KEY="dvin/UI/Error/AddAssetChildErrorCannotAddAssocAsset"/>		
		<if COND="Variables.saveerrno=-12007">
		<then>
			<XLAT.STREAM KEY="dvin/UI/Error/AddAssetChildErrorRankValue" ESCAPE="false" ENCODE="false"/>	
		</then>
		<else>
			<if COND="Variables.saveerrno=-12017">
			<then>
				<XLAT.STREAM KEY="dvin/UI/Error/AddAssetChildErrorAssociateItself" ESCAPE="false" ENCODE="false"/>	
			</then>
			<else>
				<if COND="Variables.saveerrno=-12014">
				<then>
					<XLAT.STREAM KEY="dvin/UI/Error/AddAssetChildErrorCannotBeChild" ESCAPE="false" ENCODE="false"/>	
				</then>
				<else>
					<XLAT.STREAM KEY="dvin/UI/Error/AddAssetChildErrorAssetDeleted" ESCAPE="false" ENCODE="false"/>	
					<if COND="Variables.AssetType=Collection">
					<then>
						<XLAT.STREAM KEY="dvin/UI/Error/AddAssetChildErrorCollection" ESCAPE="false" ENCODE="false"/>	
					</then>
					</if>
				</else>
				</if>
			</else>
			</if>
		</else>
		</if>
		<XLAT.STREAM KEY="dvin/UI/Error/AddAssetChildErrorUseBackButton" ESCAPE="false" ENCODE="false"/>	<br/><br/>
	</P>
	<P>
		<XLAT.STREAM KEY="dvin/UI/Error/AddAssetChildErrorContactSysAdmin" ESCAPE="false" ENCODE="false"/>
	</P>


</FTCS> 
