<p><span class="title-text"><XLAT.STREAM KEY="dvin/UI/Error/DeleteAssetTypeWarning"/></span></p>

<IF COND="Variables.uiDepAssetTypes!=Variables.empty">
<THEN>
	<XLAT.STREAM KEY="dvin/UI/Error/DeleteAssetTypeWarningAssetTypes" ESCAPE="false" ENCODE="false"/>
	<ul>
		<STRINGLIST NAME="types" STR="Variables.uiDepAssetTypes" DELIM=","/>
		<LOOP LIST="types">
			<li><STRING.STREAM VALUE="types.ITEM"/></li>
		</LOOP>
	</ul>
</THEN>
</IF>
<XLAT.STREAM KEY="dvin/UI/Error/DeleteAssetTypeWarningMsg" ESCAPE="false" ENCODE="false"/>
</FTCS> 
