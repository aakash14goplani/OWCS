<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Xcelerate/Errors/UpdateAssetRelationError
--
-- INPUT
--
-- OUTPUT
--
-->
<SETVAR NAME="saveerrno" VALUE="Variables.errno"/>
<!-- Load the information about the asset type -->

	<ASSETTYPE.LOAD NAME="type" TYPE="Variables.AssetType"/>
	<ASSETTYPE.SCATTER NAME="type" PREFIX="AssetTypeObj"/>

<!-- Display the error -->

<XLAT.STREAM KEY="dvin/UI/Admin/Error/UpdateAssetRelationError" ESCAPE="false" ENCODE="false"/>
		<if COND="Variables.saveerrno=-12017">
		<then>
		<XLAT.STREAM KEY="dvin/UI/Admin/Error/AssetSelfAssociationError" ESCAPE="false" ENCODE="false"/>
		</then>
		<else>
			<if COND="Variables.saveerrno=-12014">
			<then>
<XLAT.STREAM KEY="dvin/UI/Admin/Error/AssetTypeCanNotBeChild" ESCAPE="false" ENCODE="false"/>
			</then>
			<else>
<XLAT.STREAM KEY="dvin/UI/Admin/Error/AssetAssociationFail" ESCAPE="false" ENCODE="false"/>
				<if COND="Variables.AssetType=Collection">
				<then>
<XLAT.STREAM KEY="dvin/UI/Admin/Error/CollectionAssociationError" ESCAPE="false" ENCODE="false"/>
				</then>
				</if>
			</else>
			</if>
		</else>
		</if>
<XLAT.STREAM KEY="dvin/UI/Admin/Error/UpdateAssetRelationInstructions" ESCAPE="false" ENCODE="false"/>
</FTCS> 
