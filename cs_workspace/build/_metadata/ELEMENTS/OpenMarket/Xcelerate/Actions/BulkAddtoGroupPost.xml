<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Xcelerate/Actions/BulkAddtoGroupPost
-
- INPUT
-
- OUTPUT
-
-->

	<SUBSTITUTE STR="Variables.SelectedAssets" WHAT=";" WITH="," OUTSTR="SelectedAssets"/>
	<SETVAR NAME="id" VALUE="Variables.groupid"/>
	<!--Stuff for Chase-->
	<SETVAR NAME="calledForAskStep" VALUE="false"/>
	<SETVAR NAME="savelocalid"  VALUE="Variables.id"/>
	<IF COND="IsVariable.didCancel!=true">
		<THEN>
			<IF COND="IsVariable.TreeSelectStr=true">
				<THEN>
					<STRING.ENCODE VARIABLE="groupid" VARNAME="groupid"/>
					<CALLELEMENT NAME="OpenMarket/Xcelerate/Actions/Workflow/AddToWorkflowGroup">
						<ARGUMENT NAME="groupid" VALUE="Variables.groupid"/>
						<ARGUMENT NAME="TreeSelectStr" VALUE="Variables.TreeSelectStr"/>
					</CALLELEMENT>
				</THEN>
			</IF>
		</THEN>
	</IF>
	<SETVAR NAME="id"  VALUE="Variables.savelocalid"/>
	<IF COND="Variables.calledForAskStep=false">
		<THEN>
			<CALLELEMENT NAME="OpenMarket/Xcelerate/PrologActions/WorkflowGroupDetailsFront"/>
			<CALLELEMENT NAME="OpenMarket/Xcelerate/Actions/WorkflowGroupDetailsFront"/>
		</THEN>
	</IF>
</FTCS> 
