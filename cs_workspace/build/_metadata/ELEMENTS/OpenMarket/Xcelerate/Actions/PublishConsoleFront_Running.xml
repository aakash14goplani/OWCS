<?xml version="1.0" ?>
<!DOCTYPE ftcs SYSTEM "futuretense_cs.dtd">
<ftcs version="1.2">
<!-- OpenMarket/Xcelerate/Actions/PublishConsoleFront_Running
-
- INPUT
-
- OUTPUT
-
-->
<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/BasicEnvironment"/>
<html>
<head>
</head>
<body>
<SETVAR NAME="errno" VALUE="0"/>
<USERISMEMBER GROUP="xceleditor"/>
<if COND="Variables.errno=0">
	<!-- user is not logged in. Display logout screen -->
	<then> 
		<SATELLITE.LINK ASSEMBLER="query" PAGENAME="OpenMarket/Xcelerate/Actions/Security/TimeoutError" OUTSTRING="urlTimeoutError"/>
			<script LANGUAGE="JavaScript">
				parent.parent.location="<ics.resolvevariables NAME="Variables.urlTimeoutError"/>";
			</script>
		<throwexception/>
	</then>
</if>
<!-- Verifies if user has publishing permission. Normally this check will be called from prologactions however
PublishConsoleFront_Running is a pagelet of PublishConsoleFront and does not have related prologaction-->
<CALLELEMENT NAME="OpenMarket/Xcelerate/Actions/Security/PublishAccessCheck"/>
<if COND="Variables.doproceed=true">
<then>
<IF COND="IsSessionVariable.locale=true">
        <THEN>
            <LOCALE.CREATE VARNAME="LocaleName" LOCALENAME="SessionVariables.locale"/>
            <DATEFORMAT.CREATE NAME="_FormatDate_" DATESTYLE="FULL" TIMESTYLE="FULL" LOCALE="LocaleName"  TIMEZONEID="SessionVariables.time.zone"/>
            <DATEFORMAT.CREATE NAME="_shortFormatDate_" DATESTYLE="SHORT" TIMESTYLE="SHORT" LOCALE="LocaleName"  TIMEZONEID="SessionVariables.time.zone"/>
        </THEN>
        <ELSE>
            <DATEFORMAT.CREATE NAME="_FormatDate_" DATESTYLE="FULL" TIMESTYLE="FULL"  TIMEZONEID="SessionVariables.time.zone"/>
            <DATEFORMAT.CREATE NAME="_shortFormatDate_" DATESTYLE="SHORT" TIMESTYLE="SHORT"  TIMEZONEID="SessionVariables.time.zone"/>
        </ELSE>
</IF>
<table border="0" cellpadding="0" cellspacing="0" class="width-inner-100">
<PUBTARGETMANAGER.FIND PREFIX="pbt" PUBID="SessionVariables.pubid" FILTERFORPUBLISH="true"/>
<PUBSESSIONMANAGER.GETALLACTIVE PREFIX="pubsess:" TARGETSARRAYPREFIX="pbt"/>

<if COND="Variables.pubsess:Total=0">
<then>
	<!-- Fix for IE as IE doesn't understand min width-->
     <tr height="35px"><td NOWRAP="NOWRAP" class="width-inner-100"><XLAT.STREAM KEY="dvin/UI/NoRunningPublishSessions"/></td></tr>
</then>
<else>
    <setvar NAME="hasActiveSessions" VALUE="true" />
 	<if COND="Variables.hasActiveSessions=true">
	<then>
		<SETCOUNTER NAME="nthSession" VALUE="0"/>
		<!-- get the fresh list - since we may have failed a running one -->    
		<PUBSESSIONMANAGER.GETALLACTIVE PREFIX="pubsess:" TARGETSARRAYPREFIX="pbt"/>
	    <if COND="Variables.pubsess:Total!=0">
	    <then>
	        <tr>
	        <td>
			    <table BORDER="0" CELLSPACING="0" CELLPADDING="0" class="width-inner-100">
					<tr>
						<td/>
						<td class="tile-dark" VALIGN="TOP" HEIGHT="1">
							<IMG WIDTH="1" HEIGHT="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/>
						</td>
						<td/>
					</tr>
					<tr>
						<td class="tile-dark" VALIGN="top" WIDTH="1"><IMG WIDTH="1" HEIGHT="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/></td>
						<td>
						<SETVAR NAME="rowStyle" VALUE="pubTile-row-normal"/>
						<SETVAR NAME="separatorLine" VALUE="0"/>
						<table class="width-inner-100" cellpadding="0" cellspacing="0" border="0" bgcolor="#ffffff">
							<tr><td colspan="15" class="tile-highlight"><IMG WIDTH="1" HEIGHT="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/></td></tr>
							<tr><td class="tile-a" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;</td>
							<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir"><DIV class="new-table-title"><BR /></DIV></td>
							<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;&nbsp;&nbsp;&nbsp;</td>
							<td NOWRAP="NOWRAP" class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir"><DIV class="new-table-title"><XLAT.STREAM KEY="dvin/Common/Destination"/></DIV></td>
							<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;&nbsp;&nbsp;&nbsp;</td>
							<td NOWRAP="NOWRAP" class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir"><DIV class="new-table-title"><XLAT.STREAM KEY="dvin/UI/PublishBeginTime"/></DIV></td>
							<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;&nbsp;&nbsp;&nbsp;</td>
							<td NOWRAP="NOWRAP" class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir"><DIV class="new-table-title"><XLAT.STREAM KEY="dvin/AT/Common/Status"/></DIV></td>
							<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;&nbsp;&nbsp;&nbsp;</td>
							<td NOWRAP="NOWRAP" class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir"><DIV class="new-table-title"><XLAT.STREAM KEY="dvin/UI/PublishedBy"/></DIV></td>
							<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;&nbsp;&nbsp;&nbsp;</td>
							<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;&nbsp;&nbsp;&nbsp;</td>
							<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;&nbsp;&nbsp;&nbsp;</td>
							<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;&nbsp;&nbsp;&nbsp;</td>
							<td class="tile-c" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;</td>
						</tr>
					<loop COUNT="Variables.pubsess:Total">
	         		<PUBSESSION.GETID NAME="pubsess:Counters.nthSession" VARNAME="pubsess:id"/>
	         		<!-- make sure it is still running - if it just finished then there will be no status -->
	         		<PUBSESSIONMANAGER.STATUS ID="Variables.pubsess:id" VARNAME="pubsessionstatus"/>
					<IF COND="Variables.pubsessionstatus=running">
						<THEN>
							<setvar name="isActive" value="true"/>
						</THEN>
						<ELSE>
							<IF COND="Variables.pubsessionstatus=waiting">
								<THEN>
									<setvar name="isActive" value="true"/>
								</THEN>
								<ELSE>
									<IF COND="Variables.pubsessionstatus=cancelling">
										<THEN>
											<setvar name="isActive" value="true"/>
										</THEN>
                                        <ELSE>
                                            <IF COND="Variables.pubsessionstatus=initialized">
                                                <THEN>
                                                    <setvar name="isActive" value="true"/>
                                                </THEN>
                                            </IF>
                                        </ELSE>
									</IF>
								</ELSE>
							</IF>
						</ELSE>
					</IF>
                    <IF COND="Variables.isActive=true">
	         		<THEN>
					    <PublishClient.Status ID="Variables.pubsess:id" OUTPUT="output" />
	                	<setvar NAME="pubtgt:name" VALUE=" "/>
		                <setvar NAME="inMySite" VALUE="false"/>
		                <!-- get target name, and whether it is accessible to me in my logged on site -->
		                <PUBSESSION.GETTARGET NAME="pubsess:Counters.nthSession" VARNAME="pubsess:target"/>
		                <PUBTARGETMANAGER.LOAD ID="Variables.pubsess:target" OBJVARNAME="pubtgt"/>
						<PUBTARGET.SCATTER NAME="pubtgt" PREFIX="pubtgt:"/>
						<if COND="Variables.errno=0">
							<then>
		                     <DELIVERYTYPE.LOAD NAME="pubdt" OBJECTID="Variables.pubtgt:type"/>
							 <DELIVERYTYPE.GET NAME="pubdt" FIELD="name" OUTPUT="pubdt:name"/>
							 <DELIVERYTYPE.GET NAME="pubdt" FIELD="id" OUTPUT="pubdt:id"/>
							 <DELIVERYTYPE.GET NAME="pubdt" FIELD="dtype" OUTPUT="pubdt:dtype"/>
						     <PUBTARGET.GETSITES NAME="pubtgt" OBJVARNAME="sites"/>
		                     <SITELIST.HASSITE NAME="sites" PUBID="SessionVariables.pubid" VARNAME="inMySite"/>
							</then>
		                </if>
						<IF COND="Variables.separatorLine=1">
					<THEN>
					
			<tr>
				<!--<td colspan="15" class="light-line-color">
				<img height="1" width="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/>
				</td>-->
			</tr>
			</THEN>																		
			</IF>
			<SETVAR NAME="separatorLine" VALUE="1"/>
			<tr class="Variables.rowStyle" REPLACEALL="Variables.rowStyle">


                          <IF COND="Variables.pubdt:dtype=sitepackager">
							 <THEN>
								<SETVAR NAME="PACKAGER_OR_REALTIME" VALUE="true"/>
							 </THEN>
							 <ELSE>
								<IF COND="Variables.pubdt:dtype=realtime">
								<THEN>
									<SETVAR NAME="PACKAGER_OR_REALTIME" VALUE="true"/>
								</THEN>
								<ELSE>
									<SETVAR NAME="PACKAGER_OR_REALTIME" VALUE="false"/>
								</ELSE>
								</IF>
							 </ELSE>
							 </IF>

                        <if COND="Variables.PACKAGER_OR_REALTIME=true">
							<then>
							<SATELLITE.LINK ASSEMBLER="query" pagename="OpenMarket/Xcelerate/PrologActions/Publish/AdvPub/PublishingStatus" outstring="urlshpubInfo">
								<satellite.argument name="cs_environment" value="Variables.cs_environment"/>
								<satellite.argument name="cs_formmode" value="Variables.cs_formmode"/>
								<satellite.argument name="cs_imagedir" value="Variables.cs_imagedir"/>
								<satellite.argument name="pubsess:id" value="Variables.pubsess:id"/>
								<satellite.argument name="pubtgt:name" value="Variables.pubtgt:name"/>
								<satellite.argument name="pubtgt:id" value="Variables.pubtgt:id"/>
								<satellite.argument name="pubdt:name" value="Variables.pubdt:name"/>
								<satellite.argument name="_pubLive" value="true"/>
							</SATELLITE.LINK>
							</then>
							<else>
							<SATELLITE.LINK ASSEMBLER="query" pagename="OpenMarket/Xcelerate/Actions/ShowPublishOutputFront" outstring="urlshpubInfo">
							<satellite.argument name="cs_environment" value="Variables.cs_environment"/>
							<satellite.argument name="cs_formmode" value="Variables.cs_formmode"/>
							<satellite.argument name="id" value="Variables.pubsess:id"/>
							</SATELLITE.LINK>
							</else>
						</if>
						<td></td>
						<td></td>
						<td></td>
						<td NOWRAP="NOWRAP"><A HREF="Variables.urlshpubInfo" REPLACEALL="Variables.urlshpubInfo"><STRING.STREAM VALUE="Variables.pubtgt:name"/></A></td>
		                <td></td>
						<td NOWRAP="NOWRAP">
		                <PUBSESSION.GETSTARTDATE NAME="pubsess:Counters.nthSession" VARNAME="pubsess:startdate"/>
                        <DATEFORMAT.GETDATETIME NAME="_shortFormatDate_" VALUE="Variables.pubsess:startdate" VALUETYPE="jdbcdate" VARNAME="publishdate"/>
                        <DATEFORMAT.GETDATETIME NAME="_FormatDate_" VALUE="Variables.pubsess:startdate" VALUETYPE="jdbcdate"  VARNAME="formatteddateFullVersion"/>
                        <SPAN TITLE="Variables.formatteddateFullVersion" REPLACEALL="Variables.formatteddateFullVersion">
	 							<STRING.STREAM VARIABLE="publishdate"/>
                        </SPAN>
                            </td>
						<td></td>
		                <td NOWRAP="NOWRAP">
							<!--Cancelling status is not implemented in 7.5 release-->
							<if COND="Variables.pubsessionstatus=cancelling">
								<then>
									<XLAT.STREAM KEY="dvin/Common/Cancelling"/>
								</then>
								<else>
									<BEGINS STR="Variables.output" WHAT="Running" />
									<if COND="Variables.errno=1">
										<then>
											<XLAT.STREAM KEY="dvin/Common/Running"/>           
										</then>
										<else>
											<if COND="Variables.pubsessionstatus=waiting">
												<then>
													<XLAT.STREAM KEY="dvin/Common/Paused"/>
												</then>
                                                <else>
                                                    <if COND="Variables.pubsessionstatus=initialized">
                                                        <then>
                                                            <XLAT.STREAM KEY="dvin/Common/Initialized"/>
                                                        </then>
											</if>
										</else>
									</if>
								</else>
							</if>
								</else>
							</if>
		                    <setvar NAME="errno" VALUE="0"/>    
		                </td>
						<td></td>
		                <td NOWRAP="NOWRAP"><PUBSESSION.GETUSER NAME="pubsess:Counters.nthSession" VARNAME="pubsess:publishedby"/><STRING.STREAM VALUE="Variables.pubsess:publishedby"/></td>		
						<if COND="Variables.PACKAGER_OR_REALTIME=true">
						<then>				
						<SATELLITE.LINK ASSEMBLER="query" pagename="OpenMarket/Xcelerate/PrologActions/Publish/AdvPub/ShowPubSessionLog" outstring="pubSessionLogURL">
                        <satellite.argument name="cs_environment" value="Variables.cs_environment"/>
                        <satellite.argument name="cs_formmode" value="Variables.cs_formmode"/>
						<satellite.argument name="cs_imagedir" value="Variables.cs_imagedir"/>
						<satellite.argument name="pubsess:id" value="Variables.pubsess:id"/>
						<satellite.argument name="pubtgt:name" value="Variables.pubtgt:name"/>
						<satellite.argument name="pubtgt:id" value="Variables.pubtgt:id"/>
						<satellite.argument name="pubdt:name" value="Variables.pubdt:name"/>
						<satellite.argument name="previousPage" value="pubConsole"/>
						<satellite.argument name="action" value="getLogData"/>
                </SATELLITE.LINK>
				</then>
				<else>
					<SATELLITE.LINK ASSEMBLER="query" pagename="OpenMarket/Xcelerate/Actions/ShowPublishOutputFront" outstring="pubSessionLogURL">
					<satellite.argument name="cs_environment" value="Variables.cs_environment"/>
					<satellite.argument name="cs_formmode" value="Variables.cs_formmode"/>
					<satellite.argument name="id" value="Variables.pubsess:id"/>
                </SATELLITE.LINK>
				</else>
				</if>
		        <td/>
				<td align="center" NOWRAP="NOWRAP"><a href="Variables.pubSessionLogURL" REPLACEALL="Variables.pubSessionLogURL"><img style="position:relative;top:1px;"src="Variables.cs_imagedir/graphics/common/icon/ViewLog.gif" border="0" REPLACEALL="Variables.cs_imagedir"/><span style="position:relative;bottom:3px;margin-left:5px;"><XLAT.STREAM KEY="dvin/UI/ViewLog"/></span></a></td>
				<td></td>
					<SATELLITE.LINK assembler="query" pagename="OpenMarket/Xcelerate/PrologActions/Publish/AdvPub/PublishingStatController"  	outstring="pubStatcontrollerResumeURL">
						<satellite.argument name="cs_environment" value='Variables.cs_environment'/>
						<satellite.argument name="cs_formmode" value='Variables.cs_formmode'/>
						<satellite.argument name="pubSessionID" value='Variables.pubsess:id'/>
						<satellite.argument name="action" value="resumeSession"/>
					</SATELLITE.LINK>
					<SATELLITE.LINK assembler="query" pagename="OpenMarket/Xcelerate/PrologActions/Publish/AdvPub/PublishingStatController"  	outstring="pubStatcontrollerCancelURL">
						<satellite.argument name="cs_environment" value='Variables.cs_environment'/>
						<satellite.argument name="cs_formmode" value='Variables.cs_formmode'/>
						<satellite.argument name="pubSessionID" value='Variables.pubsess:id'/>
						<satellite.argument name="action" value="cancelSession"/>
					</SATELLITE.LINK>
						<IF COND="Variables.PACKAGER_OR_REALTIME=true">
						<THEN>
						<!--Cancelling status is not implemented in 7.5 release-->
							<IF COND="Variables.pubsessionstatus!=cancelling">
							<THEN>
									<IF COND='Variables.pubsessionstatus=waiting'>
									<THEN>
										<td>
											<XLAT.LOOKUP KEY="dvin/UI/Resume" VARNAME="_XLAT_"/>
											<a href="#" onClick="executeAction('resumeSession','Variables.pubStatcontrollerResumeURL');" onmouseover="$('resumeImg').src='Variables.cs_imagedir/graphics/common/icon/resumeHover_small.gif'" onmouseout="$('resumeImg').src='Variables.cs_imagedir/graphics/common/icon/resume_small.gif'" onmouseup="$('resumeImg').src='Variables.cs_imagedir/graphics/common/icon/resumeHover_small.gif'" onmousedown="$('resumeImg').src='Variables.cs_imagedir/graphics/common/icon/resumePressed_small.gif'" REPLACEALL="Variables.cs_imagedir,Variables.pubStatcontrollerResumeURL">
											<img id="resumeImg" title="Variables._XLAT_" border="0" src="Variables.cs_imagedir/graphics/common/icon/resume_small.gif" REPLACEALL="Variables.cs_imagedir,Variables._XLAT_"/>
											</a>&nbsp;
											<XLAT.LOOKUP KEY="dvin/UI/Cancel" VARNAME="_XLAT_"/>
											<SATELLITE.LINK assembler="query" pagename="OpenMarket/Xcelerate/PrologActions/Publish/AdvPub/PublishingStatController"  	outstring="pubStatcontrollerCancelDURL">
												<satellite.argument name="cs_environment" value='Variables.cs_environment'/>
												<satellite.argument name="cs_formmode" value='Variables.cs_formmode'/>
												<satellite.argument name="pubSessionID" value='Variables.pubsess:id'/>
												<satellite.argument name="action" value="cancelDelayedSession"/>
											</SATELLITE.LINK>
											<a href="#" onClick="executeAction('cancelDelayedSession','Variables.pubStatcontrollerCancelDURL');" onmouseover="$('cancelImg').src='Variables.cs_imagedir/graphics/common/icon/stopHover_small.gif'" onmouseout="$('cancelImg').src='Variables.cs_imagedir/graphics/common/icon/stop_small.gif'" onmouseup="$('cancelImg').src='Variables.cs_imagedir/graphics/common/icon/stopHover_small.gif'" onmousedown="$('cancelImg').src='Variables.cs_imagedir/graphics/common/icon/stopPressed_small.gif'" REPLACEALL="Variables.cs_imagedir,Variables.pubStatcontrollerCancelDURL">
											<img id="cancelImg" title="Variables._XLAT_" border="0" src="Variables.cs_imagedir/graphics/common/icon/stop_small.gif" REPLACEALL="Variables.cs_imagedir,Variables._XLAT_"/>
											</a>
										</td>
									</THEN>
									<ELSE>
										<td>
											<XLAT.LOOKUP KEY="dvin/UI/ResumeUnAvailable" VARNAME="_XLAT_"/>
											<img src="Variables.cs_imagedir/graphics/common/icon/ResumeUnavai_smalll.gif" title="Variables._XLAT_" REPLACEALL="Variables._XLAT_,Variables.cs_imagedir"/>&nbsp;
										<XLAT.LOOKUP KEY="dvin/UI/Cancel" VARNAME="_XLAT_"/>
											<a href="#" onClick="executeAction('cancelSession','Variables.pubStatcontrollerCancelURL');" onmouseover="$('cancelImg').src='Variables.cs_imagedir/graphics/common/icon/stopHover_small.gif'" onmouseout="$('cancelImg').src='Variables.cs_imagedir/graphics/common/icon/stop_small.gif'" onmouseup="$('cancelImg').src='Variables.cs_imagedir/graphics/common/icon/stopHover_small.gif'" onmousedown="$('cancelImg').src='Variables.cs_imagedir/graphics/common/icon/stopPressed_small.gif'" REPLACEALL="Variables.cs_imagedir,Variables.pubStatcontrollerCancelURL">
											<img id="cancelImg" title="Variables._XLAT_" border="0" src="Variables.cs_imagedir/graphics/common/icon/stop_small.gif" REPLACEALL="Variables.cs_imagedir,Variables._XLAT_"/>
											</a>
										</td>
										</ELSE>
										</IF>
										</THEN>
										<ELSE>
											<td>
												<XLAT.LOOKUP KEY="dvin/UI/ResumeUnAvailable" VARNAME="_XLAT_"/>
												<img src="Variables.cs_imagedir/graphics/common/icon/ResumeUnavai_smalll.gif" title="Variables._XLAT_" REPLACEALL="Variables._XLAT_,Variables.cs_imagedir"/>&nbsp;
											<XLAT.LOOKUP KEY="dvin/UI/CancelUnAvailable" VARNAME="_XLAT_"/>
												<img src="Variables.cs_imagedir/graphics/common/icon/StopUnavail_small.gif" title="Variables._XLAT_" REPLACEALL="Variables._XLAT_,Variables.cs_imagedir"/>
											</td>
										</ELSE>
										</IF>
										</THEN>
										</IF>
											<td></td></tr> 
										</THEN>
										</IF>
										<INCCOUNTER NAME="nthSession" VALUE="1"/>
						 <IF COND="Variables.rowStyle=pubTile-row-normal">
				<THEN>
					<SETVAR NAME="rowStyle" VALUE="pubTile-row-highlight"/>
				</THEN>
				<ELSE>
					<SETVAR NAME="rowStyle" VALUE="pubTile-row-normal"/>
				</ELSE>
		</IF>
	            </loop>
				</table>
	        </td>
					<td class="tile-dark" VALIGN="top" WIDTH="1"><IMG WIDTH="1" HEIGHT="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/></td>
					</tr>
					<tr>
						<td colspan="3" class="tile-dark" VALIGN="TOP" HEIGHT="1">
							<IMG WIDTH="1" HEIGHT="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/>
						</td>
					</tr>
					<tr>
						<td/>
						<td background="Variables.cs_imagedir/graphics/common/screen/shadow.gif" REPLACEALL="Variables.cs_imagedir">
							<IMG WIDTH="1" HEIGHT="5" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/>
						</td>
					<td/>
					</tr>
				</table>
			</td>
	        </tr>
		</then>
	    </if>
	</then>
	</if>
</else>
</if>
</table>
<div id="pubsess:TotalRunning" style="display:none"><STRING.STREAM VALUE="Variables.pubsess:Total"/></div>
</then></if>
</body>

    <!--<em>Import sessions:</em><br/>-->
