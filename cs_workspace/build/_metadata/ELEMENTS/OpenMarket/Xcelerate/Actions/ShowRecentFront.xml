<STRING.ENCODE VARIABLE="cs_imagedir" VARNAME="cs_imagedir"/>
<SCRIPT Language="JavaScript">
function clearAllRecent() {
<IF COND="Variables.showSiteTree=true">
<THEN>
	ClearHistory('History');
</THEN>
</IF>
parent.parent.XcelBanner.clearToolbarHistory();
parent.parent.XcelBanner.showHistory();
}
</SCRIPT>

<table border="0" cellpadding="0" cellspacing="0" class="width-outer-70">
	<tr>
		<td class="title-text"><XLAT.STREAM KEY="dvin/Common/History"/></td>
	</tr>
	<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/TitleBar">
		<ARGUMENT NAME="SpaceBelow" VALUE="No"/>
	</CALLELEMENT>
	<IF COND="IsList.theAssets=true">
	<THEN>
	<tr>
		<td><span class="small-text"><XLAT.STREAM KEY="dvin/UI/last100itemsyouhavevisited"/></span></td>
	</tr>
	<tr>
		<td><img height="10" width="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/></td>
	</tr>
	<tr>
	<td>

	<CALLELEMENT NAME="OpenMarket/Xcelerate/Actions/AssetMgt/TileMixedAssets">
			<ARGUMENT NAME="list" VALUE="theAssets"/>
			<ARGUMENT NAME="doDaysExpired" VALUE="false" />
	 </CALLELEMENT><BR/>
<XLAT.LOOKUP KEY="dvin/UI/Admin/ClearHistory" VARNAME="_ALT_"/>
<A HREF="javascript:clearAllRecent();"><CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/TextButton"><ARGUMENT NAME="buttonkey" VALUE="UI/Forms/ClearHistory"/></CALLELEMENT></A>
</td></tr>
	</THEN>
	<ELSE>
<tr>
		<td><img height="10" width="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/></td>
	</tr>
	<tr>
		<td>
			<span class="alert-color"><XLAT.STREAM KEY="dvin/UI/NoAssetslcfound"/></span><BR/>
		</td>
	</tr>
	</ELSE>
	</IF>
</table>

