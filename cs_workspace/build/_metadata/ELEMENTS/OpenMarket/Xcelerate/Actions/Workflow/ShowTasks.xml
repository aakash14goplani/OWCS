<SELECTTO LIST="ActiveWfl" FROM="WorkflowGroups" WHAT="*" ORDERBY="description"/>

<Select name="TasksFunctions" onChange="gotoTasks(this.form, TasksFunctions);">
    <option VALUE="*"/>&lt;<XLAT.STREAM KEY="dvin/UI/Wf/SelectGenWorkflowAction"/>&gt;
    <option VALUE="ShowTasks"/><XLAT.STREAM KEY="dvin/UI/Wf/ShowMyAssignments"/>
    <option VALUE="ShowCheckouts"/><XLAT.STREAM KEY="dvin/UI/Wf/ShowMyCheckouts"/>
    <option VALUE="ShowRecentFront"/><XLAT.STREAM KEY="dvin/UI/Wf/ShowRecentAssets"/>
    <option VALUE="AddNew"/><XLAT.STREAM KEY="dvin/UI/Wf/CreateaGroup"/>
    <loop LIST="ActiveWfl">
    <option VALUE="ActiveWfl.name" REPLACEALL="ActiveWfl.name"/><XLAT.STREAM KEY="dvin/UI/Wf/EditorDeleteActiveWflname"/>
    </loop>

    <setvar NAME="dofsep" VALUE="false"/>
 
</Select>

