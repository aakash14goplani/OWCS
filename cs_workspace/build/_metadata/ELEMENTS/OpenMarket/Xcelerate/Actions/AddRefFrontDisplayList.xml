<?xml version="1.0" ?>
<!DOCTYPE ftcs SYSTEM "futuretense_cs.dtd">
<ftcs version="1.2">
<!-- OpenMarket/Xcelerate/Actions/AddRefFrontDisplayList
-
- INPUT
-
- OUTPUT
-
-->
<IF COND="IsList.items=true">
    <THEN>
    <IF COND="items.#numRows!=0">
    <THEN>
    	<LOOP LIST="items">
    	<IF COND="Variables.separatorLine=1">
		<THEN>
			<tr>
        	<!--<td colspan="6" class="light-line-color"><img height="1" width="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/></td>-->
        	</tr>
        </THEN>
        </IF>
		<SETVAR NAME="separatorLine" VALUE="1"/>
		<ASSETTYPE.LIST LIST="ThisAsset" FIELD1="assettype" VALUE1="items.assettype"/>
        <tr class="Variables.rowStyle"  REPLACEALL="Variables.rowStyle"><td><BR /></td>
        <td><BR/></td><td VALIGN="TOP" NOWRAP="NOWRAP" ALIGN="LEFT">
        <DIV class="small-text-inset">
        	<STRING.STREAM VALUE="ThisAsset.description"/>
        </DIV>
        </td>
        <td><BR/></td><td VALIGN="TOP" ALIGN="LEFT">
        <DIV class="small-text-inset">
        <ics.encode base="ContentServer" session="true" output="createURL">
        <ics.argument name="pagename" value="OpenMarket/Xcelerate/Actions/NewContentFront"/>
        <ics.argument name="AssetType" value="items.assettype"/>
        <ics.argument name="StartItem" value="items.id"/>
		<if cond="IsVariable.parentid=true">
		<then>
        <ics.argument name="parentid" value="Variables.parentid"/>
        <ics.argument name="parentfield" value="Variables.parentfield"/>
        <ics.argument name="parenttype" value="Variables.parenttype"/>
		</then>
		</if>
		<if cond="IsVariable.childtypes=true">
		<then>
		<ics.argument name="childtypes" value="Variables.childtypes"/>
		</then>
		</if>
		<ics.argument name="title" value="Variables.title"/>
		<ics.argument name="assetid" value="Variables.assetid"/>
        <ics.argument name="cs_environment" value="Variables.cs_environment"/>
        <ics.argument name="memberid" value="Variables.memberid"/>
        <ics.argument name="cancelpage" value="OpenMarket/Xcelerate/Actions/AddRefFront" />
		<IF COND="IsVariable.FCKName=true">
		<THEN>
			<ics.argument name="FCKName" value="Variables.FCKName"/>
		</THEN>
		</IF>
		<if cond="Variables.IFCKEditor=true">
		<then>
			<ics.argument name="IFCKEditor" value="Variables.IFCKEditor"/>
			<ics.argument name="fielddesc" value="Variables.fielddesc"/>
			<ics.argument name="FCKAssetId" value="Variables.FCKAssetId"/>
			<ics.argument name="FCKAssetType" value="Variables.FCKAssetType"/>
			<ics.argument name="embedtype" value="Variables.embedtype"/>
			<ics.argument name="formFieldName" value="Variables.formFieldName"/>
			<ics.argument name="attrEditingStyle" value="Variables.EditingStyle"/>
		</then>
		</if>
	</ics.encode>

    <a href="Variables.createURL" REPLACEALL="Variables.createURL"><STRING.STREAM VALUE="items.name"/></a></DIV>
    </td>
    <td><BR/></td>
    </tr>
    <IF COND="Variables.rowStyle=tile-row-normal">
    <THEN><SETVAR NAME="rowStyle" VALUE="tile-row-highlight"/>
    </THEN>
    <ELSE><SETVAR NAME="rowStyle" VALUE="tile-row-normal"/>
    </ELSE>
    </IF>
    </LOOP>
    </THEN>
    </IF>
    </THEN>
    </IF>
</ftcs>