<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Xcelerate/Actions/Publish/GetEmbededPage
-
- INPUT
-
- OUTPUT
-
-->

<if COND="Variables._ESCQUOTS_!=true">
<then>
    <satellite.tag type="open"/>
    <!-- #15315 Replaced asset:load -->
	<STRING.ENCODE VARIABLE="_PAGENAME_" VARNAME="_PAGENAME_" />
	<STRING.ENCODE VARIABLE="_cid_" VARNAME="_cid_" />
	<STRING.ENCODE VARIABLE="_c_" VARNAME="_c_" />
    <ASSET.LIST LIST="lCurrentAsset" FIELD1="id" VALUE1="Variables._cid_" TYPE="Variables._c_"/>    
    <SETVAR NAME="lstatusCode" VALUE="lCurrentAsset.status"/>
    <if COND="IsError.Variables.errno!=true">
    <then>
        <if COND="Variables.lstatusCode!=VO">    <!--#15315 end-->
        <then>
            <RENDER.SATELLITEPAGE PAGENAME="Variables._PAGENAME_"
                PACKEDARGS="Variables._PACKEDARGS_" />
                    
        </then>
        <else>
            <XLAT.STREAM KEY="dvin/UI/AssetDeletedForEmbeddedPage" ENCODE="false" />
        </else>
        </if>
    </then>
    <else>
         <XLAT.STREAM KEY="dvin/UI/AssetNotExistForEmbeddedPage" ENCODE="false"/>
    </else>
    </if>
    
    <satellite.tag type="close"/>
</then>
<else>
    <XLAT.STREAM KEY="dvin/UI/CannotDisplayEmbeddedPageInsideEktron" ENCODE="false"/>
</else>
</if>

<REMOVEVAR NAME="_PACKEDARGS_"/>
</FTCS> 
