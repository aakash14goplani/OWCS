<!-- 
	Analytics Image tag 
	Data capture code for asset uses and user information
-->
<PROPERTY.GET INIFILE="futuretense_xcel.ini" PARAM="analytics.enabled" VARNAME="AnalyticsInst"/>
<IF COND="Variables.AnalyticsInst=true">
	<THEN>
	<CALLELEMENT NAME="Analytics/AddAnalyticsImgTag">
	<ARGUMENT NAME="c" VALUE="Variables.c" />
	<ARGUMENT NAME="cid" VALUE="Variables.cid " />
	<ARGUMENT NAME="site" VALUE="Variables.site" />
	<ARGUMENT NAME="recid" VALUE="Variables.recid" />
	</CALLELEMENT>
	</THEN>
</IF>