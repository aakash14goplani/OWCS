<?xml version="1.0" ?>
<!DOCTYPE ftcs SYSTEM "futuretense_cs.dtd">
<ftcs version="1.2">
<!-- OpenMarket/Xcelerate/ControlPanel/Frame/AssignmentFront
-
- INPUT
-
- OUTPUT
-
-->
<ics.getproperty NAME="xcelerate.charset" FILE="futuretense_xcel.ini" OUTPUT="fwcharset"/>
<SATELLITE.FORM ASSEMBLER="query" NAME="AssignmentConsole" METHOD="POST" id="assignmentForm">
    <input type="hidden" name="pagename" value="OpenMarket/Xcelerate/ControlPanel/ControlPanel" />
    <input type="hidden" name="mode" value="Variables.mode" REPLACEALL="Variables.mode" />
    <input type="hidden" name="wfAction" value="AssignmentPost" />
    <input type="hidden" name="_charset_" value="Variables.fwcharset" REPLACEALL="Variables.fwcharset" />

    <if COND="IsVariable.assignmentAction!=true">
    <then>
        <setvar NAME="assignmentAction" VALUE="ShowAssignments" />
    </then>
    </if>
    <callelement NAME="OpenMarket/Xcelerate/ControlPanel/Variables.assignmentAction" />

</SATELLITE.FORM>
</ftcs>
