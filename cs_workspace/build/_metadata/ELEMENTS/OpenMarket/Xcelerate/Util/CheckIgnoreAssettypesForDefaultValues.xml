<?xml version="1.0" ?>
<!DOCTYPE ftcs SYSTEM "futuretense_cs.dtd">
<ftcs version="1.2">
<!-- OpenMarket/Xcelerate/Util/CheckIgnoreAssettypes
-
- INPUT
-
- OUTPUT
-
-->
<SETVAR NAME="IgnoreATs" VALUE="AttrTypes;CSElement;Dimension;DimensionSet;ScalarVals;HistoryVals;HFields;PageAttribute;PageDefinition;PageFilter;SiteEntry;Template" />
<STRINGLIST NAME="IgnoreList" STR="Variables.IgnoreATs" DELIM=";" />
<SETVAR NAME="IgnoreAT" VALUE="false" />
<LOOP LIST="IgnoreList">
	<IF COND="IgnoreList.ITEM=Variables.AssetType">
		<THEN>
			<SETVAR NAME="IgnoreAT" VALUE="true" />
		</THEN>
	</IF>
</LOOP>
<ASSET.GETASSETTYPEPROPERTIES TYPE="Variables.item:assettype" />
<CALLELEMENT NAME="OpenMarket/Xcelerate/Util/validateFields">
	<ARGUMENT NAME="columnvalue" VALUE="Variables.AssetType"/>
	<ARGUMENT NAME="type" VALUE="String"/>
</CALLELEMENT>
<IF COND="Variables.validatedstatus=true">
<THEN>
	<EXECSQL LIST="_ATList" TABLE="ComplexAssets" SQL="SELECT assettypename, class FROM ComplexAssets where assettypename='Variables.AssetType'" />
</THEN>
</IF>
<LOOP LIST="_ATList">
	<IF COND="_ATList.class=com.openmarket.gator.attributes.AttributeManager">
		<THEN>
			<SETVAR NAME="IgnoreAT" VALUE="true" />
		</THEN>
		<ELSE>
			<IF COND="_ATList.class=com.openmarket.gator.flexfilters.FlexFilterManager">
				<THEN>
					<SETVAR NAME="IgnoreAT" VALUE="true" />
				</THEN>
				<ELSE>
					<IF COND="_ATList.class=com.openmarket.gator.flexgrouptemplates.FlexGroupTemplateManager">
						<THEN>
							<SETVAR NAME="IgnoreAT" VALUE="true" />
						</THEN>
						<ELSE>
							<IF COND="_ATList.class=com.openmarket.gator.flextemplates.FlexTemplateManager">
								<THEN>
									<SETVAR NAME="IgnoreAT" VALUE="true" />
								</THEN>
							</IF>
						</ELSE>
					</IF>
				</ELSE>
			</IF>
		</ELSE>
	</IF>
</LOOP>
</ftcs>