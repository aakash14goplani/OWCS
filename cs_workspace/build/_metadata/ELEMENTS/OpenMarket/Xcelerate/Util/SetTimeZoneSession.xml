<?xml version="1.0" ?>
<!DOCTYPE ftcs SYSTEM "futuretense_cs.dtd">
<ftcs version="1.2">
<!-- OpenMarket/Xcelerate/Util/SetTimeZoneSession
-
- INPUT
-
- OUTPUT
-
-->
<!-- if some things goes wrong at the the client javascript or some thing unexpected, This will set the server time zone as the client time zone -->
	<IF COND="IsSessionVariable.clientTimezoneOffset=false">
    <THEN>
	<if COND="IsVariable.clientTimezoneOffset=false">
	<then>
		<DATE.DEFAULTTZ VARNAME="tzone"/>
	    <SETSSVAR NAME="clientTimezoneOffset" VALUE="Variables.tzone" /> 
	</then>
	<else>
	   <SETSSVAR NAME="clientTimezoneOffset" VALUE="Variables.clientTimezoneOffset" /> 
	</else>
	</if>
	</THEN> 
	</IF>

</ftcs>