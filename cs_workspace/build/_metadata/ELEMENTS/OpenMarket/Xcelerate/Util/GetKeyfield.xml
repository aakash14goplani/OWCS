-
- DESCRIPTION
-	Get the key field value for an asset
-
- INPUT AssetType, id 
-
- OUTPUT Variables: keyfieldof
-->


<!-- return the name of the asset NOTE: calls to this should be eliminated and done in the caller -->

	<ASSET.LOAD NAME="asset" TYPE="Variables.AssetType" OBJECTID="Variables.id"/>
	<IF COND="IsError.Variables.errno=false">
		<THEN>
			<ASSET.GET NAME="asset" FIELD="name" OUTPUT="keyfieldof"/>
		</THEN>
	</IF>	
</FTCS>

