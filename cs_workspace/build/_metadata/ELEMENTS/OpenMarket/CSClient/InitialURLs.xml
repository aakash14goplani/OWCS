<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.2">
<SETVAR NAME="cs.contenttype" VALUE="text/xml; charset=utf-8"/>
<SETVAR NAME="test" VALUE="&#60;?xml version=&#34;1.0&#34; encoding=&#34;utf-8&#34;?>
"/>
<CSVAR NAME="Variables.test"/>
<!-- OpenMarket/CSClient/InitialURLs
-
- INPUT
-
- OUTPUT
-
-->
<BEGINS STR="CS.Property.ft.cgipath" WHAT="/"/>
<IF COND="Variables.errno=1">
<THEN>
    <SUBSTRING STR="CS.Property.ft.cgipath" OUTSTR="cgipathapproot" INDEX="1"/>
    <SETVAR NAME="cgipathapproot" VALUE="Variables.cgipathapprootCS.Property.ft.approot"/>
</THEN>
<ELSE>
    <SETVAR NAME="cgipathapproot" VALUE="CS.Property.ft.cgipathCS.Property.ft.approot"/>
</ELSE>
</IF>

<!-- get Content Centre build date information -->
<CALLJAVA CLASS="com.openmarket.xcelerate.util.XcelBuildDate">
   <ARGUMENT NAME="OUTPUT" VALUE="buildDate"/>
</CALLJAVA>

<STRINGLIST NAME="xxx" STR="Variables.buildDate" DELIM="%20"/>

<SETVAR NAME="lastVal" VALUE="Variables.empty"/>
<LOOP LIST="xxx">
   <IF COND="Variables.lastVal=Xcelerate">
   <THEN>
      <SETVAR NAME="version" VALUE="xxx.ITEM"/>
      <SETVAR NAME="foundVersion" VALUE="true"/>
   </THEN>
   <ELSE>
      <IF COND="Variables.lastVal=Build">
      <THEN>
         <SETVAR NAME="version" VALUE="Variables.version.xxx.ITEM"/>
      </THEN>
      </IF>
   </ELSE>
   </IF>
   <SETVAR NAME="lastVal" VALUE="xxx.ITEM"/>
</LOOP>
<IF COND="Variables.foundVersion!=true">
<THEN>
   <SETVAR NAME="version" VALUE="C Developer"/>
</THEN>
</IF>

<INITIALURLS INITIALTREEURI="Variables.cgipathapprootContentServer?pagename=OpenMarket/CSClient/CSClientPage(?)CSCPage=Initialize"
             LOGINURI="Variables.cgipathapprootContentServer?pagename=OpenMarket/CSClient/CSClientPage(?)CSCPage=Login(?)LoginUserID=(Username)(?)LoginPassword=(Password)"
             LOGOUTURI="Variables.cgipathapprootContentServer?pagename=OpenMarket/CSClient/CSClientPage(?)CSCPage=Login(?)command=logout"
             PROTOCOLVERSION="1"
             SERVERVERSION="Variables.version"
             RESTRICTEDCHARS="',&#38;#34;,;,:,?"
             REPLACEALL="Variables.cgipathapproot,Variables.version"/>

</FTCS> 
