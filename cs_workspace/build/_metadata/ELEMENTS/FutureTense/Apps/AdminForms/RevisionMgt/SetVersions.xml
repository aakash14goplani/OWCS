<?XML VERSION="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.0">
<div class="width-outer-70">
<IF COND="IsVariable.numversions=false">
     <THEN>
          <SETVAR NAME="numversions" VALUE="Variables.empty"/>
     </THEN>
</IF>

<SETCOUNTER NAME="SetVersionsCounter" VALUE="1"/>
<SETCOUNTER NAME="EndCount" VALUE="Variables.numtables"/>
<INCCOUNTER NAME="EndCount" VALUE="1"/>
<LOOP UNTIL="Counters.SetVersionsCounter=Counters.EndCount">
    <SETVAR NAME="shouldSet" VALUE="Variables.dosetversionsCounters.SetVersionsCounter"/>
    <IF COND="Variables.shouldSet=true"><THEN>
        <!-- clear out errno -->
        <SETVAR NAME="errno" VALUE="0"/>

        <!-- do the setversions -->
        <SETVAR NAME="setTable" VALUE="Variables.setmeCounters.SetVersionsCounter"/>
        <SETVERSIONS TABLE="Variables.setTable"
                     NUMVERSIONS="Variables.numversions"/>

        <!-- See if setversions succeeded -->
        <IF COND="Variables.errno=0">
             <THEN>
                  <!-- if succeeded, brag about it -->
                  <XLAT.LOOKUP KEY="dvin/AdminForms/SetVersionSucceeded" VARNAME="setVersionSucceeded"/> 
				  <CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/ShowMessage">
						<ARGUMENT NAME="msgtext" VALUE="Variables.setVersionSucceeded"/>
						<ARGUMENT NAME="severity" VALUE="info"/>
				  </CALLELEMENT>
             </THEN>
             <ELSE>
                  <!-- otherwise, call an error reporting element -->
                  <XLAT.LOOKUP KEY="dvin/AdminForms/SetVersionsfailed" VARNAME="setVersionsFailed"/>
				  <CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/ShowMessage">
						<ARGUMENT NAME="msgtext" VALUE="Variables.setVersionsFailed"/>
						<ARGUMENT NAME="severity" VALUE="info"/>
				  </CALLELEMENT>
                  <CALLELEMENT NAME="FutureTense/Apps/AdminForms/RevisionMgt/errors"/>
             </ELSE>
        </IF>
    </THEN></IF>
    <INCCOUNTER NAME="SetVersionsCounter" VALUE="1"/>
</LOOP>
</div>
</FTCS>
