			<INPUT TYPE="hidden" NAME="key" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>            
            <XLAT.LOOKUP KEY="dvin/AdminForms/Modify" VARNAME="_XLAT_"/>
