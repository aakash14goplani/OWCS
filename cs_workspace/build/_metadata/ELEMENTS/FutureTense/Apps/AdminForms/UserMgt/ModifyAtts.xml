							<IF COND="attributes.NAME!=Variables.prevAtt"><THEN>
								<STRING.STREAM VALUE="attributes.NAME"/>
								<SETVAR NAME="prevAtt" VALUE="attributes.NAME"/>
								<STRING.ENCODE LIST="attributes" COLUMN="NAME" VARNAME="OutputQString1"/>
								<INPUT TYPE="hidden" NAME="attNames" VALUE="Variables.OutputQString1" REPLACEALL="Variables.OutputQString1"/>
							</THEN><ELSE>
								<![CDATA[&nbsp;]]>
							</ELSE></IF>
						</TD>
						<TD class="form-inset" VALIGN="BASELINE">
							<STRING.ENCODE LIST="attributes" COLUMN="VALUE" VARNAME="OutputQString"/>
							<STRING.ENCODE LIST="attributes" COLUMN="NAME" VARNAME="OutputQString1"/>
							<INPUT TYPE="text" SIZE="15" NAME="Variables.OutputQString1" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString1,Variables.OutputQString"/>
							<INPUT TYPE="hidden" NAME="Variables.OutputQString1orig" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString1,Variables.OutputQString"/>
						</TD>
						<TD NOWRAP="true">
							<FONT FACE="Arial, Helvetica" SIZE="-2" COLOR="#6666cc">
								<XLAT.STREAM KEY="dvin/AdminForms/EditAttr"/> .
							</FONT>
						</TD>
						<td><BR /></td>
					</TR>
					
					</THEN></IF>
					</THEN></IF>
					</THEN></IF>
					</LOOP>
					
					<IF COND="IsVariable.prevAtt=true"><THEN>
						<TR class="Variables.rowStyle"  REPLACEALL="Variables.rowStyle"><td><BR /></td>
							<TD class="form-inset" VALIGN="BASELINE">
								<FONT FACE="Arial, Helvetica" SIZE="-1">
									<![CDATA[&nbsp;]]>
								</FONT>
							</TD>
							<TD class="form-inset" VALIGN="BASELINE">
							<STRING.ENCODE VARIABLE="prevAtt" VARNAME="prevAtt"/>
								<INPUT TYPE="text" SIZE="15" NAME="Variables.prevAtt" VALUE="" REPLACEALL="Variables.prevAtt"/>
							</TD>
							<TD NOWRAP="true">
								<FONT FACE="Arial, Helvetica" SIZE="-2" COLOR="#6666cc">
									<XLAT.STREAM KEY="dvin/AdminForms/Addnewvalue"/> .
								</FONT>
							</TD>
							<td><BR /></td>
						</TR>
						<callelement NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
					</THEN></IF>
					<IF COND="Variables.rowStyle=tile-row-normal">
					<THEN>
						<SETVAR NAME="rowStyle" VALUE="tile-row-highlight"/>
					</THEN>
					<ELSE>
						<SETVAR NAME="rowStyle" VALUE="tile-row-normal"/>
					</ELSE>
					</IF>
					<TR class="Variables.rowStyle"  REPLACEALL="Variables.rowStyle"><td><BR /></td>
						<TD class="form-inset"  VALIGN="BASELINE">
							<INPUT TYPE="text" SIZE="10" NAME="newAtt" VALUE=""/>
						</TD>
						<TD class="form-inset" VALIGN="BASELINE">
							<INPUT TYPE="text" SIZE="15" NAME="newAttValue" VALUE=""/>
						</TD>
						<TD NOWRAP="true">
							<FONT FACE="Arial, Helvetica" SIZE="-2" COLOR="#6666cc">
								<XLAT.STREAM KEY="dvin/AdminForms/Addnewattribute"/> .
							</FONT>
						</TD>
						<td><BR /></td>
					</TR>
					<TR>
						<TD COLSPAN="3" HEIGHT="5"></TD>
					</TR>
				</TABLE>
			</td><td class="tile-dark" VALIGN="top" WIDTH="1"><BR /></td>
		</tr>
		<tr>
			<td colspan="3" class="tile-dark" VALIGN="TOP" HEIGHT="1"><IMG WIDTH="1" HEIGHT="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/></td>
		</tr>
		<tr>
			<td></td><td background="Variables.cs_imagedir/graphics/common/screen/shadow.gif" REPLACEALL="Variables.cs_imagedir"><IMG WIDTH="1" HEIGHT="5" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/></td><td></td>
		</tr>
	</TABLE> 
	</td></tr>
	
	<TR>
		<TD align="left" VALIGN="TOP" COLSPAN="3">
			<INPUT TYPE="hidden" NAME="pagename" VALUE="FutureTense/Apps/AdminForms/AdminForm"/>
