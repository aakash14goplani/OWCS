<?XML VERSION="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.0">

<!-- ####################################################################### -->
<!-- ContentMgt/main                                                         -->
<!-- FutureTense, Inc.                                                       -->
<!-- ####################################################################### -->

<SATELLITE.FORM ASSEMBLER="query" ACTION="CatalogManager" METHOD="GET">
	<STRING.ENCODE VARIABLE="charset" VARNAME="OutputQString"/>
    <INPUT TYPE="HIDDEN" NAME="_charset_" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>
	<STRING.ENCODE VARIABLE="cs_environment" VARNAME="OutputQString"/>
    <INPUT TYPE="hidden"  NAME="cs_environment" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>
	<STRING.ENCODE VARIABLE="cs_formmode" VARNAME="OutputQString"/>
    <INPUT TYPE="hidden"  NAME="cs_formmode" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>
 
<table class="width-outer-70" cellspacing="0" cellpadding="0" border="0">
	<tr><td>
		<span class="title-text"><XLAT.STREAM KEY="UI/Forms/Content"/></span>
	</td></tr>
	<callelement NAME="OpenMarket/Xcelerate/UIFramework/Util/TitleBar"/>  
</table>     
<TABLE class="width-outer-30" BORDER="0" CELLPADDING="0" CELLSPACING="0">
	<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
	<TR>
		<TD class="form-label-text" VALIGN="BASELINE" NOWRAP="true">
			<span class="alert-color">*</span><XLAT.STREAM KEY="dvin/AdminForms/EnterCatalogName"/>
		</TD>
		<TD class="form-inset" VALIGN="BASELINE">
			<INPUT TYPE="text" SIZE="32" NAME="tablename" VALUE=""/>
		</TD>
		<TD NOWRAP="true" VALIGN="BASELINE"></TD>
	</TR>
	<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
	<TR>
		<TD class="form-label-text" VALIGN="BASELINE" NOWRAP="true">
			<XLAT.STREAM KEY="dvin/AdminForms/EnterCatalogKey"/>
		</TD>
		<TD class="form-inset" NOWRAP="true">
			<INPUT TYPE="text" SIZE="32" NAME="tablekey" VALUE=""/>
		</TD>
		<TD></TD>
	</TR>
	<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
	<TR>
		<TD class="form-label-text" VALIGN="BASELINE">
			<XLAT.STREAM KEY="dvin/AdminForms/EnterValueForKey"/>
		</TD>
		<TD class="form-inset">
   			<INPUT TYPE="text" SIZE="32" NAME="tablekeyvalue" VALUE=""/>
		</TD>
	</TR>
	<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
	<TR>
		<TD class="form-label-text"  VALIGN="BASELINE">
			<XLAT.STREAM KEY="dvin/AdminForms/SelectOperation"/>
		</TD>
		
		<TD class="form-inset">
			<table width="100%">
				<tr>
					<td class="form-inset" align="left">
						<INPUT TYPE="radio" style="margin-left:0px" NAME="ftchoice" VALUE="selectrow(s)" CHECKED="true"/>
					</td>
					<td align="left" valign="TOP" class="form-inset">
						<XLAT.STREAM KEY="dvin/AdminForms/QueryForContent"/>
					</td>
				</tr>
				<tr>
					<td class="form-inset" align="left" valign="TOP">
						<INPUT TYPE="radio" style="margin-left:0px" NAME="ftchoice" VALUE="addrow"/>
					</td>
					<td class="form-inset" align="left" valign="TOP">
						<XLAT.STREAM KEY="dvin/AdminForms/AddNewContent"/>
					</td>
				</tr>
				<tr>
					<td class="form-inset" align="left" valign="TOP">
						<INPUT TYPE="radio" style="margin-left:0px" NAME="ftchoice" VALUE="updaterow"/>
					</td>
					<td class="form-inset" align="left" valign="TOP">
						<XLAT.STREAM KEY="dvin/AdminForms/UpdateContent"/>
					</td>
				</tr>
				<tr>
					<td class="form-inset" align="left">
						<INPUT TYPE="radio" style="margin-left:0px" NAME="ftchoice" VALUE="replacerow"/>
					</td>
					<td class="form-inset" align="left" valign="TOP">
						<XLAT.STREAM KEY="dvin/AdminForms/ReplaceContent"/>
					</td>
				</tr>
				<tr>
					<td class="form-inset" align="left">
						<INPUT TYPE="radio" style="margin-left:0px" NAME="ftchoice" VALUE="deleterow"/>
					</td>
					<td class="form-inset" align="left" valign="TOP">
						<XLAT.STREAM KEY="dvin/AdminForms/DeleteContent"/>
					</td>
				</tr>
			</table>
		</TD>
	</TR>
	<TR>
		<TD class="form-label-text"></TD><TD class="form-inset">
			<INPUT TYPE="hidden" NAME="ftcmd" VALUE="exportform"/>
			<XLAT.LOOKUP KEY="dvin/AT/Common/OK" VARNAME="_XLAT_"/>        
  			<A onClick='if(checkExportParams(document.forms[0]) != false) {document.forms[0].submit();}' HREF="#" onmouseover="window.status='Variables._XLAT_';return true;" onmouseout="window.status='';return true;" REPLACEALL="Variables._XLAT_"><CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/TextButton"><ARGUMENT NAME="buttonkey" VALUE="dvin/AT/Common/OK"/></CALLELEMENT></A>
		</TD>
	</TR> 
</TABLE>
</SATELLITE.FORM>
</FTCS>