<?xml version="1.0" ?>
<!DOCTYPE ftcs SYSTEM "futuretense_cs.dtd">
<ftcs version="1.2">
<!-- OpenMarket/ContentServer/Installation/Deinstall
-
- INPUT
-
- OUTPUT
-
-->

<!-- This element is used to disable the cacheserver event during the uninstallation -->
<ics.clearerrno/>
<setvar name="result" value="Variables.empty"/>

<!-- We have no choice but to hardwire the name here -->
<disableevent name="CacheServer"/>

<if cond="IsError.Variables.errno=true">
<then>
	<setvar name="result" value="errno Variables.errno from deinstall of Variables.catalog"/>
</then>
</if>

<if cond="Variables.result!=Variables.empty">
<then>
	##ERROR## <csvar name="Variables.result"/><br/>
</then>
<else>
	##SUCCESS## <csvar name="Variables.result"/><br/>
</else>
</if>

</ftcs>
