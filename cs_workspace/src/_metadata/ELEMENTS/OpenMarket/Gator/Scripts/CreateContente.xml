 <Script Language="Javascript">

function createContent(id, assettype)
{
    <SATELLITE.LINK ASSEMBLER="query" PAGENAME="OpenMarket/Xcelerate/Actions/NewContentFront" OUTSTRING="urlcontentfront">
        <SATELLITE.ARGUMENT NAME="cs_environment" VALUE="Variables.cs_environment"/>
        <SATELLITE.ARGUMENT NAME="cs_formmode" VALUE="Variables.cs_formmode"/>
    </SATELLITE.LINK>
	 var pURL = "<ics.resolvevariables NAME="Variables.urlcontentfront"/>";
    <![CDATA[
    pURL=pURL+'&AssetType='+assettype+'&id='+id;
	parent.frames['XcelAction'].location=pURL;
    ]]>
}
</Script>
</FTCS> 
