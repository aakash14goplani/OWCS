<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Gator/FlexibleAssets/Common/CheckAttributeDisplayOrder
-
- INPUT
-
- OUTPUT
-
-->

<setvar NAME="errno" VALUE="0"/>
<setvar NAME="attrDisplayOrder" VALUE="first"/>

<PROPERTY.GET INIFILE="gator.ini" PARAM="cc.attrDisplayOrder" VARNAME="LocalLoadDispOrder"/>
<if COND="Variables.errno!=0">
	<then>
		<br/><XLAT.STREAM KEY="dvin/Error/UI/NogatoriniFile" errno="Variables.errno" EVALALL="false"/> 
	</then>
	<else>
		<if COND="Variables.LocalLoadDispOrder=first">
			<then>
				<setvar NAME="attrDisplayOrder" VALUE="first"/>
			</then>
		</if>
		<REMOVEVAR NAME="LocalLoadDispOrder"/>
	</else>
</if>

</FTCS> 
