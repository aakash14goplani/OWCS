<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Gator/FlexibleAssets/Common/RatingListTable
--
-- INPUT
--
-- OUTPUT
--
-->

<setvar NAME="errno" VALUE="0"/>
<BEGINS STR="Variables.SegDefRating" WHAT="Variables."/>
<IF COND="Variables.errno=1">
<THEN>
	<setvar NAME="SegDefRating"  VALUE="Variables.empty"/>
</THEN>
</IF>
		 
<if COND="IsList.RatingList=true">
<then>
	<if COND="RatingList.#numRows!=0">
	<then>
		<loop LIST="RatingList">
			<setvar NAME="RatingList.assetid"  VALUE="RatingList.rating"/>
			<setvar NAME="NOTRatingList.assetid" VALUE="RatingList.NOTrating"/>
		</loop>
	</then>
	</if>
</then>
</if>

  
<tr>
	<td class="form-label-text" valign="top"><XLAT.STREAM KEY="dvin/FlexibleAssets/Common/Ratings"/>:</td>
	<td></td>
	<td class="form-inset">
		<if COND="IsList.Variables.list=true">
		<then>
		<if COND="Variables.list.#numRows!=0">
		<then>
		<table style="min-width:600px" border="0" cellpadding="0" cellspacing="0">
			<tr>
				<td></td><td class="tile-dark" HEIGHT="1"><IMG WIDTH="1" HEIGHT="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/></td><td></td>
	        </tr>
			<tr> 
			<td></td>
				<td>
					<table class="width-inner-100" border="0" cellpadding="0" cellspacing="0" bgcolor="#ffffff">
						<tr><td colspan="9" class="tile-highlight"><IMG WIDTH="1" HEIGHT="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/></td></tr>
						<tr>
							<td class="tile-a" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir"><br/></td>
							<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;&nbsp;&nbsp;&nbsp;</td>
							<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir"><DIV class="new-table-title"><XLAT.STREAM KEY="dvin/FlexibleAssets/Common/Segment"/></DIV></td>
							<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;&nbsp;&nbsp;&nbsp;</td>
							<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir"><DIV class="new-table-title"><XLAT.STREAM KEY="dvin/FlexibleAssets/Common/Insegment"/></DIV></td>
							<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;&nbsp;&nbsp;&nbsp;</td>
							<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir"><DIV class="new-table-title"><XLAT.STREAM KEY="dvin/FlexibleAssets/Common/OutofSegment"/></DIV></td>
							<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;&nbsp;&nbsp;&nbsp;</td>
							<td class="tile-c" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir"><br/></td>
						</tr>
						<tr><td colspan="9" class="tile-dark"><IMG WIDTH="1" HEIGHT="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/></td></tr>
						<tr>
							<td class="tile-dark" VALIGN="top" WIDTH="1" NOWRAP="nowrap"></td>
							<td colspan="7"><img src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" width="1" height="5" border="0" REPLACEALL="Variables.cs_imagedir"/></td>
							<td class="tile-dark" VALIGN="top" WIDTH="1" NOWRAP="nowrap"></td>
						</tr>
						<loop LIST="Variables.list">
							<tr>
								<td class="tile-dark" VALIGN="top" WIDTH="1" NOWRAP="nowrap"><BR /></td>	
								<td><BR/></td>		
								<td><span ><STRING.STREAM VALUE="Variables.list.name"/></span></td>
								<td><BR/></td>	

								<IF COND="Variables.displaytype=input">
								<THEN>
									<setvar NAME="segrating" VALUE="Variables.Variables.list.assetid"/>
											
									<setvar NAME="errno" VALUE="0"/>
									<BEGINS STR="Variables.segrating" WHAT="Variables."/>
									<IF COND="Variables.errno=1">
									<THEN>
										<setvar NAME="segrating" VALUE="Variables.empty"/>
										<setvar NAME="NOTsegrating" VALUE="Variables.empty"/>
									</THEN>
									</IF>
											
									<setvar NAME="NOTsegrating" VALUE="Variables.NOTVariables.list.assetid"/>
									<setvar NAME="errno" VALUE="0"/>
									<BEGINS STR="Variables.NOTsegrating" WHAT="Variables."/>
									<IF COND="Variables.errno=1">
									<THEN>
										<setvar NAME="NOTsegrating" VALUE="Variables.empty"/>
									</THEN>
									</IF>
											
									<td class="small-text-box">
										<setvar NAME="RateName" VALUE="Variables.RateName*Rank-Variables.list.assetid!"/>
										<callelement NAME="OpenMarket/Gator/AttributeTypes/TextBox">
											<argument NAME="inputName" VALUE="Rank-Variables.list.assetid"/>
											<argument NAME="inputValue" VALUE="Variables.segrating"/>
											<argument NAME="inputSize" VALUE="3"/>
											<argument NAME="inputMaxlength" VALUE="3"/>
											<argument NAME="width" VALUE="15em"/>
										</callelement>
									</td>
									<td></td>
									<td class="small-text-box">
										<setvar NAME="RateName" VALUE="Variables.RateName*Rank-Variables.list.assetidNOT!"/>
										<callelement NAME="OpenMarket/Gator/AttributeTypes/TextBox">
											<argument NAME="inputName" VALUE="Rank-Variables.list.assetidNOT"/>
											<argument NAME="inputValue" VALUE="Variables.NOTsegrating"/>
											<argument NAME="inputSize" VALUE="3"/>
											<argument NAME="inputMaxlength" VALUE="3"/>
											<argument NAME="width" VALUE="15em"/>
										</callelement>
									</td>
							 
								</THEN>
								<ELSE>
									<td align="center">
										<STRING.STREAM VALUE="Variables.list.rating"/>
									</td>
									<td></td>
									<td align="center">
										<STRING.STREAM VALUE="Variables.list.NOTrating"/>
									</td>
								</ELSE>
								</IF>
								<td></td>
								<td class="tile-dark" VALIGN="top" WIDTH="1" NOWRAP="nowrap"><BR /></td>
							</tr>
							<tr>
								<td class="tile-dark" VALIGN="top" WIDTH="1" NOWRAP="nowrap"></td>
								<td colspan="7"><img src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" width="1" height="5" border="0" REPLACEALL="Variables.cs_imagedir"/></td>
								<td class="tile-dark" VALIGN="top" WIDTH="1" NOWRAP="nowrap"></td>
							</tr>
						</loop>
		                 <tr>
		                 <td colspan="9" class="tile-dark" VALIGN="TOP" HEIGHT="1"><IMG WIDTH="1" HEIGHT="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/></td>
		                 </tr>
		                 <tr>
							<td colspan="9"><img src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" width="1" height="5" border="0" REPLACEALL="Variables.cs_imagedir"/></td>
						</tr>
						<tr>
							<td><BR/></td>	
							<td><BR/></td>		
							<td><span ><XLAT.STREAM KEY="dvin/FlexibleAssets/Common/NoSegmentRate"/></span>:</td>
							<td><BR/></td>
							<if COND="IsVariable.displaytype=true">
							<then>
									<td align="left" class="small-text-box" colspan="5">
										<callelement NAME="OpenMarket/Gator/AttributeTypes/TextBox">
											<argument NAME="inputName" VALUE="SegDefRating"/>
											<argument NAME="inputValue" VALUE="Variables.SegDefRating"/>
											<argument NAME="inputSize" VALUE="3"/>
											<argument NAME="inputMaxlength" VALUE="3"/>
											<argument NAME="width" VALUE="15em"/>
										</callelement>
									</td>
								<setvar NAME="RateName" VALUE="Variables.RateName*SegDefRating!"/>
							</then>
							<else>
								<if COND="IsVariable.SegDefRating=true">
								<then>
									<if COND="Variables.SegDefRating!=Variables.empty">
									<then>
										<td colspan="5"><STRING.STREAM VALUE="Variables.SegDefRating"/></td>
									</then>
									</if>
								</then>
								</if>
							</else>
							</if>
						</tr>
					</table>
				</td>
				<td></td>
               </tr>
		</table>
		</then>
		<else>
			<span class="disabledText"><XLAT.STREAM KEY="dvin/FlexibleAssets/Common/NoSegmentDef"/></span>
		</else>
		</if>
		</then>
		<else>
		    <span class="disabledText"><XLAT.STREAM KEY="dvin/FlexibleAssets/Common/NoSegmentDef"/></span>
		</else>
		</if>
	 </td>
</tr>
</FTCS> 
