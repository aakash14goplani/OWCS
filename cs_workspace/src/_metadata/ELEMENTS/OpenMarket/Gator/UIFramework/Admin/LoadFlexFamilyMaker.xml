<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Gator/UIFramework/Admin/LoadFlexFamilyMaker
-
- INPUT
-
- OUTPUT
-
-->

<!-- This element handles all children of the FlexFamilyMaker root node -->
<!-- All we need to do is 'Add New' and any Flex Assets -->

<IF COND="Variables.Depth=1">
<THEN>

	<REMOVEVAR NAME="LoadURL"/>
	<REMOVEVAR NAME="OKActions"/>
	<REMOVEVAR NAME="Description"/>
	<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
		<ARGUMENT NAME="AdHoc" VALUE="FFM-AddNewFamily"/>
		<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
	</CALLELEMENT>

	<XLAT.LOOKUP KEY="dvin/TreeApplet/AddNewFamily" ENCODE="false" VARNAME="_XLAT_"/>
	<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
		<ARGUMENT NAME="Label" VALUE="Variables._XLAT_"/>
		<ARGUMENT NAME="Description" VALUE="Variables._XLAT_"/>
		<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
		<ARGUMENT NAME="OpURL" VALUE="ContentServer?pagename=OpenMarket/Gator/UIFramework/TreeOpURL&#38;AssetType=Variables.AssetType"/>
		<ARGUMENT NAME="ExecuteURL" VALUE="ContentServer?pagename=OpenMarket/Xcelerate/Admin/FlexAssetMakerFront&#38;action=new"/>
		<ARGUMENT NAME="Image" VALUE="Xcelerate/OMTree/TreeImages/plus.png"/>
	</CALLELEMENT>

	<!-- now show any existing FlexAttributes -->
	<ATM.GETASSETS FILTER="com.openmarket.gator.interfaces.IAttributeManager" LISTVARNAME="currentTypes"/>
	<IF COND="IsList.currentTypes=true">
	<THEN>
		<IF COND="currentTypes.#numRows!=0">
		<THEN>
			<REMOVEVAR NAME="OKActions"/>
			<REMOVEVAR NAME="Description"/>
			
			<!-- Attribute types always have a LoadURL since the either have a filter or will have the Add New Filter node -->
			
			<LOOP LIST="currentTypes">
				<IF COND="currentTypes.assettype!=PageAttribute">
				<THEN>
				<ASSETTYPE.LIST LIST="currentType" FIELD1="assettype" VALUE1="currentTypes.assettype"/>

				<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
					<ARGUMENT NAME="AdHoc" VALUE="FFM-currentType.assettype"/>
					<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
				</CALLELEMENT>

				<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
					<ARGUMENT NAME="Label" VALUE="currentType.assettype"/>
					<ARGUMENT NAME="Description" VALUE="currentType.description"/>
					<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
					<ARGUMENT NAME="ExecuteURL" VALUE="ContentServer?pagename=OpenMarket/Xcelerate/Admin/AssetTypeFront&#38;action=details&#38;assettype=currentType.assettype"/>
					<ARGUMENT NAME="LoadURL" VALUE="ContentServer?pagename=OpenMarket/Gator/UIFramework/LoadAdminTree&#38;op=load&#38;NodeType=FlexFamilyMaker&#38;currentType=currentTypes.assettype&#38;Depth=2"/>
					<ARGUMENT NAME="OpURL" VALUE="ContentServer?pagename=OpenMarket/Gator/UIFramework/TreeOpURL&#38;AssetType=Variables.AssetType"/>
					<ARGUMENT NAME="Image" VALUE="Xcelerate/OMTree/TreeImages/AssetTypes/currentTypes.assettype.png"/>
					<ARGUMENT NAME="RefreshKeys" VALUE="FFM-currentType.assettype"/>
				</CALLELEMENT>
				</THEN>
				</IF>
			</LOOP>
		</THEN>
		</IF>
	</THEN>
	</IF>
</THEN>
<ELSE>
	<!-- Depth = 2 means under an Attribute -->
	<IF COND="Variables.Depth=2">
	<THEN>
		
		<REMOVEVAR NAME="LoadURL"/>
		<REMOVEVAR NAME="OKActions"/>
		<REMOVEVAR NAME="Description"/>
		
		<!-- show any parent defs for current attr -->
		<IF COND="IsList.lParentDefs=true">
		<THEN>
			<FLUSH LIST="lParentDefs"/>
		</THEN>
		</IF>
		
		<SETVAR NAME="tablename" VALUE="FlexGrpTmplTypes"/>
		<CALLELEMENT NAME="OpenMarket/Xcelerate/Util/validateFields">
			<ARGUMENT NAME="columnvalue" VALUE="Variables.currentType"/>
			<ARGUMENT NAME="type" VALUE="String"/>
		</CALLELEMENT>
		<IF COND="Variables.validatedstatus=true">
		<THEN>
			<EXECSQL SQL="SELECT * FROM Variables.tablename WHERE assetattr='Variables.currentType'" LIST="lParentDefs"/>
		</THEN>
		</IF>
		
		<IF COND="IsList.lParentDefs=true">
		<THEN>
			<IF COND="lParentDefs.#numRows!=0">
			<THEN>
				<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
				<ARGUMENT NAME="AdHoc" VALUE="FFM-AddNewPD"/>
				<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
				</CALLELEMENT>

				<XLAT.LOOKUP KEY="dvin/TreeApplet/AddNewParentDefAT" ENCODE="false" VARNAME="_XLAT_"/>
				<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
				<ARGUMENT NAME="Label" VALUE="Variables._XLAT_"/>
				<ARGUMENT NAME="Description" VALUE="Variables._XLAT_"/>
				<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
				<ARGUMENT NAME="OpURL" VALUE="ContentServer?pagename=OpenMarket/Gator/UIFramework/TreeOpURL&#38;AssetType=Variables.AssetType"/>
				<ARGUMENT NAME="ExecuteURL" VALUE="ContentServer?pagename=OpenMarket/Xcelerate/Admin/FlexFamilyFront&#38;op=make&#38;parent=Variables.currentType&#38;member=parentdef"/>
				<ARGUMENT NAME="Image" VALUE="Xcelerate/OMTree/TreeImages/plus.png"/>
				</CALLELEMENT>
			</THEN>
			</IF>
		</THEN>
		</IF>
		
		<IF COND="IsList.lFilterType=true">
		<THEN>
			<FLUSH LIST="lFilterType"/>
		</THEN>
		</IF>
		<SETVAR NAME="tablename" VALUE="FlexFilterTypes"/>
		<CALLELEMENT NAME="OpenMarket/Xcelerate/Util/validateFields">
			<ARGUMENT NAME="columnvalue" VALUE="Variables.currentType"/>
			<ARGUMENT NAME="type" VALUE="String"/>
		</CALLELEMENT>
		<IF COND="Variables.validatedstatus=true">
		<THEN>
			<EXECSQL SQL="SELECT * FROM Variables.tablename WHERE filterattr='Variables.currentType'" LIST="lFilterType"/>
		</THEN>
		</IF>
		
		<IF COND="lFilterType.#numRows=0">
		<THEN>
			<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
				<ARGUMENT NAME="AdHoc" VALUE="FFM-AddFilter"/>
				<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
			</CALLELEMENT>

			<XLAT.LOOKUP KEY="dvin/TreeApplet/AddNewFilterAT" ENCODE="false" VARNAME="_XLAT_"/>
			<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
				<ARGUMENT NAME="Label" VALUE="Variables._XLAT_"/>
				<ARGUMENT NAME="Description" VALUE="Variables._XLAT_"/>
				<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
				<ARGUMENT NAME="OpURL" VALUE="ContentServer?pagename=OpenMarket/Gator/UIFramework/TreeOpURL&#38;AssetType=Variables.AssetType"/>
				<ARGUMENT NAME="ExecuteURL" VALUE="ContentServer?pagename=OpenMarket/Xcelerate/Admin/FlexFamilyFront&#38;op=make&#38;member=filter&#38;parent=Variables.currentType"/>
				<ARGUMENT NAME="Image" VALUE="Xcelerate/OMTree/TreeImages/plus.png"/>
			</CALLELEMENT>
		</THEN>
		</IF>

		<IF COND="IsList.lParentDefs=true">
		<THEN>
			<LOOP LIST="lParentDefs">
				<ASSETTYPE.LIST LIST="currentType" FIELD1="assettype" VALUE1="lParentDefs.assettype"/>

				<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
					<ARGUMENT NAME="AdHoc" VALUE="FFM-currentType.assettype"/>
					<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
				</CALLELEMENT>

				<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
					<ARGUMENT NAME="Label" VALUE="currentType.assettype"/>
					<ARGUMENT NAME="Description" VALUE="currentType.description"/>
					<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
					<ARGUMENT NAME="ExecuteURL" VALUE="ContentServer?pagename=OpenMarket/Xcelerate/Admin/AssetTypeFront&#38;action=details&#38;assettype=currentType.assettype"/>
					<ARGUMENT NAME="LoadURL" VALUE="ContentServer?pagename=OpenMarket/Gator/UIFramework/LoadAdminTree&#38;op=load&#38;NodeType=FlexFamilyMaker&#38;parentDef=lParentDefs.assettype&#38;currentType=currentType.assettype&#38;Depth=3"/>
					<ARGUMENT NAME="OpURL" VALUE="ContentServer?pagename=OpenMarket/Gator/UIFramework/TreeOpURL&#38;AssetType=Variables.AssetType"/>
					<ARGUMENT NAME="Image" VALUE="Xcelerate/OMTree/TreeImages/AssetTypes/currentType.assettype.png"/>
					<ARGUMENT NAME="RefreshKeys" VALUE="FFM-currentType.assettype"/>
				</CALLELEMENT>
			</LOOP>
		</THEN>
		</IF>
		
		<IF COND="IsList.lFilterType=true">
		<THEN>
			<IF COND="lFilterType.#numRows!=0">
			<THEN>
				<REMOVEVAR NAME="LoadURL"/>
				<ASSETTYPE.LIST LIST="currentType" FIELD1="assettype" VALUE1="lFilterType.filtertype"/>

				<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
					<ARGUMENT NAME="AdHoc" VALUE="FFM-currentType.assettype"/>
					<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
				</CALLELEMENT>

				<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
					<ARGUMENT NAME="Label" VALUE="currentType.assettype"/>
					<ARGUMENT NAME="Description" VALUE="currentType.description"/>
					<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
					<ARGUMENT NAME="ExecuteURL" VALUE="ContentServer?pagename=OpenMarket/Xcelerate/Admin/AssetTypeFront&#38;action=details&#38;assettype=currentType.assettype"/>
					<ARGUMENT NAME="OpURL" VALUE="ContentServer?pagename=OpenMarket/Gator/UIFramework/TreeOpURL&#38;AssetType=Variables.AssetType"/>
					<ARGUMENT NAME="Image" VALUE="Xcelerate/OMTree/TreeImages/AssetTypes/currentType.assettype.png"/>
				</CALLELEMENT>
			</THEN>
			</IF>
		</THEN>
		</IF>
		
	</THEN>
	<ELSE>
		<!-- Depth = 3 means under a parent def -->
		<IF COND="Variables.Depth=3">
		<THEN>
			
			<REMOVEVAR NAME="LoadURL"/>
			<REMOVEVAR NAME="OKActions"/>
			<REMOVEVAR NAME="Description"/>

			<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
				<ARGUMENT NAME="AdHoc" VALUE="FFM-AddNewFD"/>
				<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
			</CALLELEMENT>

			<XLAT.LOOKUP KEY="dvin/TreeApplet/AddNewFlexDefAT" ENCODE="false" VARNAME="_XLAT_"/>
			<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
				<ARGUMENT NAME="Label" VALUE="Variables._XLAT_"/>
				<ARGUMENT NAME="Description" VALUE="Variables._XLAT_"/>
				<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
				<ARGUMENT NAME="OpURL" VALUE="ContentServer?pagename=OpenMarket/Gator/UIFramework/TreeOpURL&#38;AssetType=Variables.AssetType"/>
				<ARGUMENT NAME="ExecuteURL" VALUE="ContentServer?pagename=OpenMarket/Xcelerate/Admin/FlexFamilyFront&#38;op=make&#38;member=def&#38;parent=Variables.currentType"/>
				<ARGUMENT NAME="Image" VALUE="Xcelerate/OMTree/TreeImages/plus.png"/>
			</CALLELEMENT>
			
			<!-- show any flex defs for current parent def -->
			<SETVAR NAME="tablename" VALUE="FlexTmplTypes"/>
			<CALLELEMENT NAME="OpenMarket/Xcelerate/Util/validateFields">
				<ARGUMENT NAME="columnvalue" VALUE="Variables.currentType"/>
				<ARGUMENT NAME="type" VALUE="String"/>
			</CALLELEMENT>
			<IF COND="Variables.validatedstatus=true">
			<THEN>
				<EXECSQL SQL="SELECT * FROM Variables.tablename WHERE assettemplate='Variables.currentType'" LIST="lFlexDefs"/>
			</THEN>
			</IF>
			
			<IF COND="IsError.Variables.errno=false">
			<THEN>
				<LOOP LIST="lFlexDefs">
					<ASSETTYPE.LIST LIST="currentType" FIELD1="assettype" VALUE1="lFlexDefs.assettype"/>


					<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
						<ARGUMENT NAME="AdHoc" VALUE="FFM-currentType.assettype"/>
						<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
					</CALLELEMENT>

					<STRING.ENCODE VARIABLE="parentDef" VARNAME="parentDef"/>
					<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
						<ARGUMENT NAME="Label" VALUE="currentType.assettype"/>
						<ARGUMENT NAME="Description" VALUE="currentType.description"/>
						<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
						<ARGUMENT NAME="ExecuteURL" VALUE="ContentServer?pagename=OpenMarket/Xcelerate/Admin/AssetTypeFront&#38;action=details&#38;assettype=currentType.assettype"/>
						<ARGUMENT NAME="LoadURL" VALUE="ContentServer?pagename=OpenMarket/Gator/UIFramework/LoadAdminTree&#38;op=load&#38;NodeType=FlexFamilyMaker&#38;parentDef=Variables.parentDef&#38;currentType=currentType.assettype&#38;Depth=4"/>
						<ARGUMENT NAME="OpURL" VALUE="ContentServer?pagename=OpenMarket/Gator/UIFramework/TreeOpURL&#38;AssetType=Variables.AssetType"/>
						<ARGUMENT NAME="Image" VALUE="Xcelerate/OMTree/TreeImages/AssetTypes/currentType.assettype.png"/>
						<ARGUMENT NAME="RefreshKeys" VALUE="FFM-currentType.assettype"/>
					</CALLELEMENT>
				</LOOP>
			</THEN>
			</IF>
		</THEN>
		<ELSE>
			<!-- Depth = 4 means under a flex def -->
			<IF COND="Variables.Depth=4">
			<THEN>
				
				<REMOVEVAR NAME="LoadURL"/>
				<REMOVEVAR NAME="OKActions"/>
				<REMOVEVAR NAME="Description"/>


				<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
					<ARGUMENT NAME="AdHoc" VALUE="FFM-AddNewParent"/>
					<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
				</CALLELEMENT>

				<XLAT.LOOKUP KEY="dvin/TreeApplet/AddNewParentAT" ENCODE="false" VARNAME="_XLAT_"/>
				<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
					<ARGUMENT NAME="Label" VALUE="Variables._XLAT_"/>
					<ARGUMENT NAME="Description" VALUE="Variables._XLAT_"/>
					<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
					<ARGUMENT NAME="OpURL" VALUE="ContentServer?pagename=OpenMarket/Gator/UIFramework/TreeOpURL&#38;AssetType=Variables.AssetType"/>
					<ARGUMENT NAME="ExecuteURL" VALUE="ContentServer?pagename=OpenMarket/Xcelerate/Admin/FlexFamilyFront&#38;op=make&#38;member=parent&#38;parent=Variables.currentType"/>
					<ARGUMENT NAME="Image" VALUE="Xcelerate/OMTree/TreeImages/plus.png"/>
				</CALLELEMENT>
				
				<!-- show any parents for this defs parent def -->
				<SETVAR NAME="tablename" VALUE="FlexGroupTypes"/>
				<CALLELEMENT NAME="OpenMarket/Xcelerate/Util/validateFields">
					<ARGUMENT NAME="columnvalue" VALUE="Variables.parentDef"/>
					<ARGUMENT NAME="type" VALUE="String"/>
				</CALLELEMENT>
				<IF COND="Variables.validatedstatus=true">
				<THEN>
					<EXECSQL SQL="SELECT * FROM Variables.tablename WHERE assettemplate='Variables.parentDef'" LIST="lFlexParents"/>
				</THEN>
				</IF>
				
				<IF COND="IsError.Variables.errno=false">
				<THEN>
					<LOOP LIST="lFlexParents">
						<ASSETTYPE.LIST LIST="currentType" FIELD1="assettype" VALUE1="lFlexParents.assettype"/>

						<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
							<ARGUMENT NAME="AdHoc" VALUE="FFM-currentType.assettype"/>
							<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
						</CALLELEMENT>


						<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
							<ARGUMENT NAME="Label" VALUE="currentType.assettype"/>
							<ARGUMENT NAME="Description" VALUE="currentType.description"/>
							<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
							<ARGUMENT NAME="ExecuteURL" VALUE="ContentServer?pagename=OpenMarket/Xcelerate/Admin/AssetTypeFront&#38;action=details&#38;assettype=currentType.assettype"/>
							<ARGUMENT NAME="LoadURL" VALUE="ContentServer?pagename=OpenMarket/Gator/UIFramework/LoadAdminTree&#38;op=load&#38;NodeType=FlexFamilyMaker&#38;currentType=currentType.assettype&#38;Depth=5&#38;def=Variables.currentType"/>
							<ARGUMENT NAME="OpURL" VALUE="ContentServer?pagename=OpenMarket/Gator/UIFramework/TreeOpURL&#38;AssetType=Variables.AssetType"/>
							<ARGUMENT NAME="Image" VALUE="Xcelerate/OMTree/TreeImages/AssetTypes/currentType.assettype.png"/>
							<ARGUMENT NAME="RefreshKeys" VALUE="FFM-currentType.assettype"/>
						</CALLELEMENT>
					</LOOP>
				</THEN>
				</IF>
			</THEN>
			<ELSE>
				<!-- Depth = 5 means under a Flex Parent -->
				<IF COND="Variables.Depth=5">
				<THEN>
					
					<REMOVEVAR NAME="LoadURL"/>
					<REMOVEVAR NAME="OKActions"/>
					<REMOVEVAR NAME="Description"/>
					<XLAT.LOOKUP KEY="dvin/TreeApplet/AddNewFlexAT" ENCODE="false" VARNAME="_XLAT_"/>

					<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
						<ARGUMENT NAME="AdHoc" VALUE="FFM-AddNewFlexAsset"/>
						<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
					</CALLELEMENT>
					<STRING.ENCODE VARIABLE="def" VARNAME="def"/>
					<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
						<ARGUMENT NAME="Label" VALUE="Variables._XLAT_"/>
						<ARGUMENT NAME="Description" VALUE="Variables._XLAT_"/>
						<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
						<ARGUMENT NAME="OpURL" VALUE="ContentServer?pagename=OpenMarket/Gator/UIFramework/TreeOpURL&#38;AssetType=Variables.AssetType"/>
						<ARGUMENT NAME="ExecuteURL" VALUE="ContentServer?pagename=OpenMarket/Xcelerate/Admin/FlexFamilyFront&#38;op=make&#38;member=asset&#38;parent=Variables.currentType&#38;def=Variables.def"/>
						<ARGUMENT NAME="Image" VALUE="Xcelerate/OMTree/TreeImages/plus.png"/>
					</CALLELEMENT>
					
					<!-- show any parents for this defs parent def -->
					<SETVAR NAME="tablename" VALUE="FlexAssetTypes"/>
					<CALLELEMENT NAME="OpenMarket/Xcelerate/Util/validateFields">
						<ARGUMENT NAME="columnvalue" VALUE="Variables.currentType"/>
						<ARGUMENT NAME="type" VALUE="String"/>
					</CALLELEMENT>
					<IF COND="Variables.validatedstatus=true">
					<THEN>
						<EXECSQL SQL="SELECT * FROM Variables.tablename WHERE assetgroup='Variables.currentType'" LIST="lFlexAssets"/>
					</THEN>
					</IF>
					<IF COND="IsError.Variables.errno=false">
					<THEN>
						<LOOP LIST="lFlexAssets">
							<ASSETTYPE.LIST LIST="currentType" FIELD1="assettype" VALUE1="lFlexAssets.assettype"/>


							<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNodeID" SCOPED="STACKED">
								<ARGUMENT NAME="AdHoc" VALUE="FFM-currentType.assettype"/>
								<ARGUMENT NAME="TreeNodeID" VALUE="Variables.empty"/>
							</CALLELEMENT>

							<CALLELEMENT NAME="OpenMarket/Gator/UIFramework/BuildTreeNode">
								<ARGUMENT NAME="Label" VALUE="currentType.assettype"/>
								<ARGUMENT NAME="Description" VALUE="currentType.description"/>
								<ARGUMENT NAME="ID" VALUE="Variables.TreeNodeID"/>
								<ARGUMENT NAME="ExecuteURL" VALUE="ContentServer?pagename=OpenMarket/Xcelerate/Admin/AssetTypeFront&#38;action=details&#38;assettype=currentType.assettype"/>
								<ARGUMENT NAME="OpURL" VALUE="ContentServer?pagename=OpenMarket/Gator/UIFramework/TreeOpURL&#38;AssetType=Variables.AssetType"/>
								<ARGUMENT NAME="Image" VALUE="Xcelerate/OMTree/TreeImages/AssetTypes/currentType.assettype.png"/>
							</CALLELEMENT>
						</LOOP>
					</THEN>
					</IF>
				</THEN>
				</IF>
			</ELSE>
			</IF>
		</ELSE>
		</IF>
	</ELSE>
	</IF>
</ELSE>
</IF>
</FTCS> 
