<SETVAR NAME='_IS_TREE_ELEMENT_' VALUE='true'/>
<TTM.LOAD OBJVARNAME="tab" ID="Variables.tabid"/>
<TREETAB.SCATTER NAME="tab" PREFIX="Saved:"/>
<table class="width-outer-70" cellspacing="0" cellpadding="0" border="0">
	<tr><td>
		<span class="title-text"><XLAT.STREAM KEY="dvin/UI/Admin/EditTreetabtitle" Saved:title="Variables.Saved:title"/></span>
	</td></tr>
	<callelement NAME="OpenMarket/Xcelerate/UIFramework/Util/TitleBar"/>
</table>

<IF COND="Variables.errno=0">
<THEN>
	<INPUT TYPE="hidden" NAME="Edit:id" VALUE="Variables.Saved:id" REPLACEALL="Variables.Saved:id"/>
	<INPUT TYPE="hidden" NAME="Edit:ordinal" VALUE="Variables.Saved:ordinal" REPLACEALL="Variables.Saved:ordinal"/>
	<INPUT TYPE="hidden" NAME="Edit:globalitemsitemTotal" VALUE="3"/>
	<INPUT TYPE="hidden" NAME="Edit:rolesitemTotal" VALUE="1"/>


    <table BORDER="0" CELLSPACING="0" CELLPADDING="0" class="width-outer-70">
	<tr><td>
		<table class="width-inner-100" cellpadding="0" cellspacing="0" border="0" bgcolor="#ffffff">
			<callelement NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
			<tr>
				<td class="form-label-text">
					<SPAN CLASS="alert-color">*</SPAN><XLAT.STREAM KEY="dvin/UI/Title"/>:
				</td>
				<td class="form-inset">
					<SUBSTITUTE STR="Variables.Saved:title" WHAT="&#34;" WITH="&#38;quot;" OUTSTR="OutputQString"/>
