<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Gator/Util/GetStoreId
--
-- INPUT
--
-- OUTPUT
--
-->
<setvar NAME="errno" VALUE="0"/>
<setvar NAME="CurrentStoreId" VALUE="Variables.empty"/>
<setvar NAME="txnumStores" VALUE="0"/>
<COMMERCEENGINE.GETSTORES LISTVARNAME="lStoreIDs"/>

<IF COND="IsList.lStoreIDs=true">
<THEN>
  <IF COND="lStoreIDs.#numRows!=0">
  <THEN>
    <setvar NAME="CurrentStoreId" VALUE="lStoreIDs.value"/>
    <setvar NAME="txnumStores" VALUE="lStoreIDs.#numRows"/>
    <setcounter NAME="indexcounter" VALUE="0"/>
    <LOOP LIST="lStoreIDs">
      <setvar NAME="Counters.indexcounterCurrentStoreId" VALUE="lStoreIDs.value"/>
      <inccounter NAME="indexcounter"  VALUE="1"/>
    </LOOP>
  </THEN>
  </IF>
</THEN>
</IF>
<SETVAR NAME="errno" VALUE="0"/>

</FTCS> 
