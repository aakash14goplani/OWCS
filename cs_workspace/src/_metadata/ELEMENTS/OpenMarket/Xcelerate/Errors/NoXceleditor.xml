<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/BasicEnvironment"/>
<html>
<head>
 <title><XLAT.STREAM KEY="dvin/UI/Error/CSAuthError" ESCAPE="false" ENCODE="false"/></title>
 <CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/StandardHeader"/>
 <link href="Variables.cs_imagedir/../js/fw/css/ui/transition.css" rel="styleSheet" type="text/css" REPLACEALL="Variables.cs_imagedir,Variables.cs_imagedir"/>
 <replaceall list="SessionVariables.WemUI">
 <script type="text/javascript">
 function loginAgain()
 {
	var WemUI = 'SessionVariables.WemUI';
	var url = 'Variables.cs_imagedir/../';
	if(WemUI=='true')
	{
		url = url + 'login/';
	}
	else
	{
		/* for delivery install login link is different */
		url = url + 'Xcelerate/LoginPage.html';
	}
	window.location.href=url;
 }
 </script>
 </replaceall>
</head>
<body class="fw">
<div class="centerboxcontainer" id="container">
 <div class="centerbox">
  <div class="loginbox" id="loginbox">
   <div class="topshadow"></div>
   <div class="leftshadow"></div>
   <div class="rightshadow"></div>
   <div class="bottomshadow"></div>
   <div class="loginBevelBottom"></div>
   <div class="loginheader"><div class="item logoWebCenterSites"></div></div>
   <div class="logincontent">
    <div class="logo"></div>
    <div class="loginform">
     <div class="right"><b><XLAT.STREAM KEY="dvin/UI/Error/NotAuthorized" ESCAPE="false" ENCODE="false"/></b></div>
	 <div class="clear"></div>
     <div class="pt30"></div>
     <div class="clear"></div>
     <div class="left"><XLAT.STREAM KEY="dvin/UI/Error/NotAuthorizedForSite" ESCAPE="false" ENCODE="false"/></div>
	 <div class="clear"></div>
     <div class="pt10"></div>
     <div class="left"><XLAT.STREAM KEY="dvin/UI/Error/AuthorizationHint" ESCAPE="false" ENCODE="false"/></div>
     <div class="clear"></div>
     <div class="pt40"></div>
     <div class="left"><XLAT.STREAM KEY="dvin/UI/Error/ContactSysAdmin" ESCAPE="false" ENCODE="false"/></div>
     <div class="clear"></div>
	 <div class="loginAgain">
	  <a id="loginAgain" href="javascript:loginAgain()"><XLAT.STREAM KEY="UI/ErrorHTML/LoginAgain" ESCAPE="false" ENCODE="false"/></a>
	 </div>
    </div>
   </div>
  </div>
  <div class="center">Copyright (c) 2012, Oracle and/or its affiliates. All rights reserved.</div>
 </div>
</div>
<div class="loginscreen"></div>
<div class="loginscreenbg"></div>
</body>
</html>
<PROPERTY.GET PARAM="wem.enabled" INIFILE="futuretense_xcel.ini" VARNAME="wemenabled"/>
<if cond="Variables.wemenabled=true">
	<then>
		<CALLELEMENT NAME="fatwire/wem/integration/processSSOLogout"/>
	</then>
</if>
<CATALOGMANAGER>
	<ARGUMENT NAME="ftcmd" VALUE="logout"/>
	<ARGUMENT NAME="killsession" VALUE="true"/>
</CATALOGMANAGER>
</FTCS>