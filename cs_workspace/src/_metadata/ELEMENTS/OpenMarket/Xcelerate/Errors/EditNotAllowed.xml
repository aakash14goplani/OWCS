<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Xcelerate/Errors/EditNotAllowed
-
- INPUT
-  Variables.errno
-  Variables.AssetType
-  Variables.id
- OUTPUT
-
-->
<setvar NAME="errorcode" VALUE="Variables.errno"/>

<ASSETTYPE.LOAD NAME="type" TYPE="Variables.AssetType"/>
<ASSETTYPE.SCATTER NAME="type" PREFIX="at"/>
<!-- publish is in progress, edit prohibited -->
<if COND="Variables.errorcode=-12034">
<then>
<XLAT.STREAM KEY="dvin/UI/Error/EditNotAllowedErrCode12034" ESCAPE="false" ENCODE="false"/>
</then>
<else>
	<!-- publish is in progress, display a warning -->
	<if COND="Variables.errorcode=-12069">
	<then>
		<XLAT.STREAM KEY="dvin/UI/Error/EditNotAllowedErrCode12069" ESCAPE="false" ENCODE="false"/>
	</then>
	<else>			
		<!-- unknown error -->
		<XLAT.STREAM KEY="dvin/UI/Error/EditNotAllowed" ESCAPE="false" ENCODE="false"/>
	</else>
	</if>
</else>
</if>

</FTCS> 
