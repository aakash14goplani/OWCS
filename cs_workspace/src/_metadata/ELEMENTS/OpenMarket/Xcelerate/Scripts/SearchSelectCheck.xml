<SCRIPT LANGUAGE="JavaScript">
<![CDATA[
function doAutoCheck(obj,name)
{
val = "Select"+name;
if ( obj.form.elements[name].value != "")
	obj.form.elements[val].checked = true;
else
	obj.form.elements[val].checked = false;
}
   
