<Script Language="Javascript">

function clearAssignment(id, assettype)
{
    <SATELLITE.LINK ASSEMBLER="query" pagename="OpenMarket/Xcelerate/Actions/ClearAssignmentFront" outstring="urlclrassgntfront">
        <satellite.argument name="cs_environment" value="Variables.cs_environment"/>
      <satellite.argument name="cs_formmode" value="Variables.cs_formmode"/>
    </SATELLITE.LINK>
    <replace STR="Variables.urlclrassgntfront">
    var pURL = "Variables.urlclrassgntfront";
    </replace>
    <![CDATA[
    pURL=pURL+'&AssetType='+assettype+'&id='+id;
	parent.frames['XcelAction'].location=pURL;
    ]]>
}

</Script>

