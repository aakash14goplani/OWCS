<if cond="Variables.cs_environment=ucform">
<then>
	<script Language="javascript">
	function showRevision(revnum, assetId , assetType) {
		SitesApp.event('active', 'revision', {rev: revnum});
	}
	</script>
</then>
<else>
	<Script Language="Javascript">

	function showRevision(pURL,revnum)
	{
		 revWindow = window.open(pURL, "Rev"+revnum, "width=480,height=550,scrollbars=yes,toolbar=no,location=no,status=yes,menubar=no,resizable=yes,directories=no");
		 revWindow.focus();
	}

	</Script>
</else>
