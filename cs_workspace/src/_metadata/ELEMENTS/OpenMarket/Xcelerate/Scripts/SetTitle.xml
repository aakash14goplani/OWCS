    <SCRIPT Language="JavaScript">
            function writeTitle(name)
            {
                if (window.name!='XcelAction') {
                    top.name=name;
                    document.write('<TITLE>'+name+'</TITLE>');
                }
                
            }
    </SCRIPT>
    <SCRIPT Language="JavaScript">
        <if COND="IsVariable.AssetType=true">
            <then>
                <STRING.ENCODE VARIABLE="AssetType" VARNAME="OutputQString"/>
                <replaceall LIST="Variables.OutputQString">
                writeTitle('Variables.OutputQString');
                </replaceall>
            </then>
            <else>
                writeTitle('XcelAction');
            </else>
        </if>
    </SCRIPT>

</FTCS>
