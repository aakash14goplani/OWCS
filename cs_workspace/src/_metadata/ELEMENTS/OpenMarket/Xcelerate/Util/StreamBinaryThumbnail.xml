<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.2">
<!-- OpenMarket/Xcelerate/Util/StreamBinaryThumbnail
-
- INPUT
-	Variables.mimetype
-  Variables.AssetType
-  Variables.id
-  Variables.nthThumbnail
-  Variables.Revision
- OUTPUT
-
-->
<STRING.ENCODE VARIABLE="id" VARNAME="id"/>
<URL.UNPACK VALUE="%0D%0A" VARNAME="CRLF"/>
<ICS.STREAMHEADER NAME="Content-Type" VALUE="Variables.mimetype"/>

<IF COND="IsVariable.Revision=true">
<THEN>
   <ASSET.LOADREVISION TYPE="Variables.AssetType" OBJECTID="Variables.id" NAME="ainst" REVISION="Variables.Revision"/>
</THEN>
<ELSE>
   <ASSET.LOAD NAME="ainst" TYPE="Variables.AssetType" OBJECTID="Variables.id"/>
</ELSE>
</IF>
<ASSET.SCATTER NAME="ainst" PREFIX="a" FIELDLIST="Thumbnail"/>
<STRING.STREAMBINARY CGI="a:Thumbnail:Variables.nthThumbnail:thumb"/>

