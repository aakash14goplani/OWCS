<?xml version="1.0" ?>
<!DOCTYPE ftcs SYSTEM "futuretense_cs.dtd">
<ftcs version="1.2">
<!-- OpenMarket/Xcelerate/Util/LoadRevisionBlob
-
- INPUT
-
- OUTPUT
-
-->
   <ICS.STREAMHEADER NAME="Content-Type" VALUE="Variables.mimetype"/>
      <ICS.STREAMHEADER NAME="Content-Disposition" VALUE="inline; filename=Variables.filename"/>
      <ICS.STREAMHEADER NAME="MDT-Type" VALUE="abinary; charset=UTF-8"/>
      
      <IF COND="IsVariable.revision=true">
      <THEN>
         <ASSET.LOADREVISION TYPE="Variables.assettype" OBJECTID="Variables.id" NAME="ainst" REVISION="Variables.revision"/>
      </THEN>
      <ELSE>
         <ASSET.LOAD NAME="ainst" TYPE="Variables.assettype" FIELD="id" VALUE="Variables.id"/>
      </ELSE>
      </IF>
      <ASSET.SCATTER NAME="ainst" PREFIX="a" />
      <STRING.STREAMBINARY CGI="a:Variables.fieldname"/>

</ftcs>