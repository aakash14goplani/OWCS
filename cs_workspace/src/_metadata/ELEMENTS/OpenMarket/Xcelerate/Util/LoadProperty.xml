<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Xcelerate/Util/LoadProperty
-
- INPUT
- 	varname = name of variable to create with property value
-  property = name of property to load
-  ini = name of ini file
- OUTPUT
-
-->

<setvar NAME="errno" VALUE="0"/>
<setvar NAME="Variables.varname" VALUE="Variables.empty"/>

<LOADPROPERTY FILE="futuretense.ini;Variables.ini"/>
<if COND="Variables.errno!=0">
<then>
	<br/>Loading of futuretense.ini;Variables.ini had error <csvar NAME="Variables.errno"/>
</then>
<else>
	<setvar NAME="LocalVar" VALUE="CS.Property.Variables.property"/>
	<setvar NAME="Variables.varname" VALUE="Variables.LocalVar"/>
	<setvar NAME="errno" VALUE="0"/>
	<RESTOREPROPERTY CLOSE="false"/>
	<REMOVEVAR NAME="LocalVar"/>
</else>
</if>

</FTCS> 
