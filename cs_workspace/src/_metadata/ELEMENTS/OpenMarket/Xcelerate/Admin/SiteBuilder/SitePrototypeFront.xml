	<SCRIPT LANGUAGE="JavaScript">
	<![CDATA[
			
function setAll( directive )
{
	var obj = document.forms[0].elements[0];	
	
	var i = 0;
	while ( i < obj.form.elements['numAssetTypes'].value )
	{
		if ( obj.form.elements['sb:replicatespecs:' + i + ':copymode'].length > 0 )
		{
			if ( directive=="N")
			{
				obj.form.elements['sb:replicatespecs:' + i + ':copymode'][0].checked=true;
				obj.form.elements['sb:replicatespecs:' + i + ':copymode'][1].checked=false;
				if (obj.form.elements['sb:replicatespecs:' + i + ':copymode'][2])
					obj.form.elements['sb:replicatespecs:' + i + ':copymode'][2].checked=false;
			}
			else if ( directive=="C")
			{
				obj.form.elements['sb:replicatespecs:' + i + ':copymode'][0].checked=false;
				obj.form.elements['sb:replicatespecs:' + i + ':copymode'][1].checked=true;
				if (obj.form.elements['sb:replicatespecs:' + i + ':copymode'][2])
					obj.form.elements['sb:replicatespecs:' + i + ':copymode'][2].checked=false;
			}
			else if ( directive=="S")
			{
				obj.form.elements['sb:replicatespecs:' + i + ':copymode'][0].checked=false;
				obj.form.elements['sb:replicatespecs:' + i + ':copymode'][1].checked=false;
				if (obj.form.elements['sb:replicatespecs:' + i + ':copymode'][2])
					obj.form.elements['sb:replicatespecs:' + i + ':copymode'][2].checked=true;
			}
		}
		i++;
	}
}
	]]>
