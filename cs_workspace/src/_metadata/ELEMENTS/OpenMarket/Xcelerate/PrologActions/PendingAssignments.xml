    <!-- check whther user is allowed to edit SiteCatalog or ElementCatalog -->
    <SETVAR NAME="hasEditACL" VALUE="true"/>
    <USERISMEMBER GROUP="PageEditor"/>
    <if COND="Variables.errno=0">
    <then>
        <setvar NAME="hasEditACL" VALUE="WrongACLToEditAsset"/>
    </then>
    <else>
        <USERISMEMBER GROUP="ElementEditor"/>
        <if COND="Variables.errno=0">
        <then>
            <setvar NAME="hasEditACL" VALUE="WrongACLToEditAsset"/>
        </then>
        </if>
    </else>
    </if>
      
<IF COND="IsVariable.users=false">
<THEN>
<USERMANAGER.GETLOGINUSER VARNAME="users"/>
</THEN>
</IF>
<CALLELEMENT NAME="OpenMarket/Xcelerate/PrologActions/WorkflowReportPost">
	<ARGUMENT NAME="report_assignto" VALUE="user"/>
	<ARGUMENT NAME="users" VALUE="Variables.users"/>
	<ARGUMENT NAME="status" VALUE="queued,abstain"/>
	<ARGUMENT NAME="report_assets" VALUE="all"/>
	<ARGUMENT NAME="report_state" VALUE="all"/>
</CALLELEMENT>

