<?xml version="1.0" ?>
<!DOCTYPE ftcs SYSTEM "futuretense_cs.dtd">
<ftcs version="1.2">
<!-- OpenMarket/Xcelerate/ControlPanel/StartMenuAssetSubtype
-
-
- INPUT
-	Variables.startmenupename - Stratmenu name
-	Variables.assettypename - current assettype name 
- 	Variables.simplesearch - is this simple search or advanced search? 
-
- OUTPUT
-	flextemplateid - Subtype id (associated with Startmenu) 
-->

<!-- Load start menu item  and get subtype. -->
<STARTMENU.LOAD NAME="Variables.startmenupename" ITEMTYPE="Search" OBJVARNAME="stmenuobj"/>
<STARTMENUITEM.GETASSETSUBTYPE NAME="stmenuobj" VARNAME="assetsubtype"/>


<IF COND="IsVariable.assetsubtype=true">
<THEN>
    <fatm.gettemplatetype ASSETTYPE="Variables.assettypename" VARNAME="templatetype"/>
    <ASSET.LIST TYPE="Variables.templatetype" FIELD1="name" VALUE1="Variables.assetsubtype" LIST="result"/>
    <IF COND="Variables.simplesearch=true">
    <THEN>
	<SETVAR NAME="flextemplateid" VALUE="result.id" REPLACEALL="result.id"/>
    </THEN>
    </IF>
</THEN>
</IF>

</ftcs>