<if COND="IsVariable.style=true">
<then>
    <![CDATA[
    <SPAN id="Variables.beginrefid" STYLE="Variables.style">            
    ]]>
</then>
<else>
    <![CDATA[
    <SPAN id="Variables.beginrefid">            
    ]]>
</else>
</if>

<![CDATA[
<SCRIPT>
    initCollection('Variables.parentid','Variables.parenttype','Variables.parentfield','Variables.assetid','Variables.assettype');
</SCRIPT>

<table cellspacing="0" border="0">
<tr id="bar_Variables.beginrefid">           
<td>
