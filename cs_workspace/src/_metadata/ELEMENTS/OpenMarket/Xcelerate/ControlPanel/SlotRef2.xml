    <then>  
        <img src="Variables.cs_imagedir/graphics/SessionVariables.locale/controlpanel/swap.gif" class="sb_swap" border="0" id="change_Variables.beginrefid" title="Variables._button_change_title" onclick="site_changeref('Variables.parentid','Variables.parenttype','Variables.parentfield','Variables.assettype','Variables.assetid',Variables.lockenabled);" />
    </then>
    </if>
   
    <asset.CanTakeAction NAME="theAsset" ACTIONS="moveto" FIELD="Variables.parentfield" MEMBERID="Variables.assetid" VARNAME="canMove" />
