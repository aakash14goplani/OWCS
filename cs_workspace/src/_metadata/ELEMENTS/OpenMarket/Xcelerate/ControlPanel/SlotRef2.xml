    <then>
        <img src="Variables.cs_imagedir/graphics/common/controlpanel/up_noright.gif" class="sb_up" border="0" id="up_Variables.beginrefid" title="Variables._button_moveup_title" onclick="site_up('Variables.parentid','Variables.parenttype', 'Variables.parentname','Variables.beginrefid', Variables.lockenabled);" />
        <img src="Variables.cs_imagedir/graphics/common/controlpanel/dn_noright.gif" class="sb_down" border="0" id="down_Variables.beginrefid" title="Variables._button_movedown_title" onclick="site_down('Variables.parentid','Variables.parenttype', 'Variables.parentname','Variables.beginrefid',Variables.lockenabled);" />
