<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Xcelerate/ControlPanel/Login
-
- INPUT
-
- OUTPUT
-
-->

<PROPERTY.GET PARAM="wem.enabled" INIFILE="futuretense_xcel.ini" VARNAME="wemenabled"/>
 <if cond="Variables.wemenabled=true">
 <then>
	<CALLELEMENT NAME="OpenMarket/Xcelerate/ControlPanel/Login/ProcessWemLogin"/>
</then>
<else>
	<CALLELEMENT NAME="OpenMarket/Xcelerate/ControlPanel/Login/ProcessLogin"/>
</else>

</if>		

<IF COND="IsVariable.panel=false">
<THEN>
	<SETVAR NAME="panel" VALUE="FullPanel"/>
</THEN>
</IF>

<SETVAR NAME="errno" VALUE="0"/>
<USERISMEMBER GROUP="xceleditor"/>

<IF COND="Variables.errno=1">
<THEN>
	<CALLELEMENT NAME="OpenMarket/Xcelerate/ControlPanel/Login/Variables.panelLogin"/>
</THEN>
<ELSE> <!-- Variables.errno != 1 -->	
	<CALLELEMENT NAME="OpenMarket/Xcelerate/ControlPanel/Login/Variables.panelError"/>  	
</ELSE>
</IF> <!-- End if Variables.errno = 1 -->
</FTCS>
