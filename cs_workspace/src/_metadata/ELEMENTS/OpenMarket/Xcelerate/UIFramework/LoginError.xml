<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<PROPERTY.GET PARAM="xcelerate.charset" INIFILE="futuretense_xcel.ini" VARNAME="charset"/>
<SETVAR NAME="cs.contenttype" VALUE="text/html; charset=Variables.charset"/>

<!-- 
$Logfile: /VerticalApps/XcelerateC/install/Xcelerate/Populate/ElementCatalog/OpenMarket/Xcelerate/UIFramework/LoginError.xml $ 
$Revision: 7 $ 
$Modtime: 3/02/04 4:26a $ 
-->
<!-- OpenMarket/Xcelerate/UIFramework/LoginError
-
- INPUT
-	(optional) destpage link back to login page
- OUTPUT
-
-->
<STRING.ENCODE VARIABLE="cs_imagedir" VARNAME="cs_imagedir"/>
<STRING.ENCODE VARIABLE="cs_formmode" VARNAME="cs_formmode"/>
<STRING.ENCODE VARIABLE="cs_environment" VARNAME="cs_environment"/>
<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/BasicEnvironment"/>

<HTML>
	<HEAD>
		<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/StandardHeader"/>
		<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/SetStylesheet"/>
	</HEAD>
	<BODY>
		<DIV STYLE="margin-top : 100px" ALIGN="CENTER">
		<if COND="IsVariable.destpage=false">
		<then>
			<SETVAR NAME="destpage" VALUE="Variables.cs_imagedir/LoginPage.html"/>
		</then>
		</if>
		<callelement NAME="OpenMarket/Xcelerate/Actions/Util/ShowError">
			<argument NAME="error" VALUE="LoginError"/>
			<argument NAME="destpage" VALUE="Variables.destpage"/>
		</callelement>
	</DIV>
	</BODY>
</HTML>
</FTCS> 
