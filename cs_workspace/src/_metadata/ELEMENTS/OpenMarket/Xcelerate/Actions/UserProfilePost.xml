<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Xcelerate/Actions/UserProfilePost
-
- INPUT
-
- OUTPUT
-
-->

	<IF COND="Variables.selectedLocale=None">
	<THEN>
		<SETVAR NAME="selectedLocale" VALUE="Variables.empty"/>
	</THEN>
	</IF>
	
	<USERMANAGER.GETLOGINUSER VARNAME="loginuser"/>
	<USERMANAGER.GETUSER USER="Variables.loginuser" OBJVARNAME="userobj"/>
	<CCUSER.SETLOCALE NAME="userobj" VALUE="Variables.selectedLocale"/>
	<USERMANAGER.SETUSER OBJECT="userobj"/>
	
	<IF COND="Variables.errno!=0">
	<THEN>
		<XLAT.STREAM KEY="dvin/UI/Admin/Error/Errorsavinguserpreference"/><BR/>
	</THEN>
	</IF>
	
	<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/ApplicationPage">
		<ARGUMENT NAME="ThisPage" VALUE="UserProfileFront"/>
		<ARGUMENT NAME="PostPage" VALUE="UserProfilePost"/>
		<ARGUMENT NAME="action" VALUE="details"/>
	</CALLELEMENT>

</FTCS> 
