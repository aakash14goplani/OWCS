<?XML VERSION="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.0">

<!-- ####################################################################### -->
<!-- CatalogMgt/Create                                                       -->
<!-- FutureTense, Inc.                                                       -->
<!-- ####################################################################### -->

<SATELLITE.FORM ASSEMBLER="query" ACTION="CatalogManager" METHOD="post" onSubmit="return checkAddInput(this)">
    <STRING.ENCODE VARIABLE="charset" VARNAME="OutputQString"/>
	<INPUT TYPE="HIDDEN" NAME="_charset_" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>
	<STRING.ENCODE VARIABLE="cs_environment" VARNAME="OutputQString"/>
	<INPUT TYPE="hidden"  NAME="cs_environment" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>
	<STRING.ENCODE VARIABLE="cs_formmode" VARNAME="OutputQString"/>
	<INPUT TYPE="hidden"  NAME="cs_formmode" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>

<IF COND="IsVariable.key=true"><THEN>
    <IF COND="Variables.key!=%"><THEN>
        <SETSSVAR NAME="catalogkey" VALUE="Variables.key"/>
    </THEN></IF>
</THEN><ELSE>
    <SETVAR NAME="key" VALUE="Variables.empty"/>
</ELSE></IF>
<table class="width-outer-70" cellspacing="0" cellpadding="0" border="0">
	<tr><td>
		<span class="title-text"><XLAT.STREAM KEY="dvin/AdminForms/AddCatalog"/></span>
	</td></tr>
	<callelement NAME="OpenMarket/Xcelerate/UIFramework/Util/TitleBar"/>
</table>
<TABLE class="width-outer-70" BGCOLOR="#FFFFFF" BORDER="0" CELLPADDING="0" CELLSPACING="0">
   
    <TR>
    	<TD class="form-label-inset" align="left" NOWRAP="true" COLSPAN="3">
    		<XLAT.STREAM KEY="dvin/AdminForms/CatalogInformation"/>
    	</TD>
    </TR>
	<callelement NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>	
    <TR>
        <TD class="form-label-text" align="right" NOWRAP="true">
         	<span class="alert-color">*</span> <XLAT.STREAM KEY="dvin/AdminForms/CatalogName"/>:
        </TD>
        <TD class="form-inset" align="left">
			<STRING.ENCODE VARIABLE="key" VARNAME="OutputQString"/>
			<REPLACE STR="Variables.OutputQString">
				<INPUT TYPE="text" NAME="tablename" SIZE="32" VALUE="Variables.OutputQString"/>                 
			</REPLACE>
    	</TD>
 		<TD></TD> 
    </TR>
	<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
    <TR>
        <TD class="form-label-text" align="right" VALIGN="TOP" NOWRAP="true">
  			<span class="alert-color">*</span><XLAT.STREAM KEY="dvin/AdminForms/CatalogType"/>:
        </TD>
        <TD class="form-inset" align="left">
			<SELECT NAME="systable" SIZE="2">
				<OPTION VALUE="no" SELECTED="true"/><XLAT.STREAM KEY="dvin/AdminForms/ContentCatalog"/>
				<OPTION VALUE="obj"/><XLAT.STREAM KEY="dvin/AdminForms/ObjectCatalog"/>
			</SELECT>
    	</TD>
  		<TD></TD> 
    </TR>
	<callelement NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>	
    <TR>
        <TD class="form-label-text" align="right" NOWRAP="true">
   			<span class="alert-color">*</span><XLAT.STREAM KEY="dvin/AdminForms/FileStorageDirectory"/>
        </TD>
        <TD class="form-inset" align="left">
   			<INPUT TYPE="text" NAME="uploadDir" SIZE="32" VALUE=""/>
    	</TD>
		<TD></TD> 
    </TR>
	<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
    <TR>
        <TD class="form-label-text" align="right" VALIGN="TOP" NOWRAP="true">
            <XLAT.STREAM KEY="dvin/AdminForms/AccessPrivileges"/>:
        </TD>
        <TD class="form-inset" align="left">
    		<INPUT TYPE="HIDDEN" NAME="aclList" VALUE=""/>
            <CALLELEMENT NAME="FutureTense/Apps/AdminForms/Common/ACLList"/>
        </TD>
 	<TD></TD> 
    </TR>
	<callelement NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
	<TR>
    	<TD class="form-label-inset" align="left" NOWRAP="true" COLSPAN="3">
    		<XLAT.STREAM KEY="dvin/AdminForms/CatalogDefinition"/>
    	</TD>
    </TR>
    <TR>
    	<TD class="form-inset" align="left" COLSPAN="3">
			<XLAT.STREAM KEY="dvin/AdminForms/EnterFieldNameAndType"/>
                <P>
		<XLAT.STREAM KEY="dvin/AdminForms/EnterPrimaryKeyForContentCatalog"/>
                </P>

                <P>
		<XLAT.STREAM KEY="dvin/AdminForms/DoNotEnterPrimaryKeyForObjectCatalog"/>
                 </P>
    	</TD>
    </TR>
	<callelement NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>	
    <TR>
    	<TD align="left" COLSPAN="3">
    		<TABLE BGCOLOR="#FFFFFF" BORDER="0" CELLPADDING="0" CELLSPACING="0">
    		<tr><td>
				<TABLE  BORDER="0" CELLSPACING="0" CELLPADDING="0">
					<tr>
						<td></td>
						<td class="tile-dark" HEIGHT="1">
							<IMG WIDTH="1" HEIGHT="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" 
							REPLACEALL="Variables.cs_imagedir"/> 
						</td>
						<td></td>
					</tr>
					<tr>
						<td class="tile-dark" VALIGN="top" WIDTH="1"><BR/></td>
						<td> 
							<TABLE width="100%" cellpadding="0" cellspacing="0" border="0" bgcolor="#ffffff">
								<tr>
									<td colspan="7" class="tile-highlight"><IMG WIDTH="1" HEIGHT="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/>
									</td>
								</tr>
								<tr colspan="2">
									<td class="tile-a" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir"><DIV class="new-table-title">&nbsp;&nbsp;&nbsp;&nbsp;<XLAT.STREAM KEY="dvin/AdminForms/FieldName"/></DIV>
									</td>
									<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir"><DIV class="new-table-title">&nbsp;&nbsp;&nbsp;&nbsp;<XLAT.STREAM KEY="dvin/AdminForms/FieldType"/></DIV></td>
								</tr>	
								<SCRIPT LANGUAGE="JavaScript">
									<XLAT.STREAM KEY="dvin/AdminForms/generateUserFields()" ENCODE="false" ESCAPE="true" />;
								</SCRIPT>
							</TABLE>
						</td><td class="tile-dark" VALIGN="top" WIDTH="1"><BR /></td>
					</tr>
					<tr>
						<td colspan="3" class="tile-dark" VALIGN="TOP" HEIGHT="1"><IMG WIDTH="1" HEIGHT="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/></td>
					</tr>
					<tr>
						<td></td><td background="Variables.cs_imagedir/graphics/common/screen/shadow.gif" REPLACEALL="Variables.cs_imagedir"><IMG WIDTH="1" HEIGHT="5" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/></td><td></td>
					</tr>
				</TABLE> 
			</td></tr>
	   		</TABLE>
    	</TD>
    </TR>
    <TR>
        <TD align="LEFT" COLSPAN="3">
   			<INPUT TYPE="hidden" NAME="ftcmd" VALUE="createtable"/>
			<XLAT.LOOKUP KEY="dvin/UI/CSAdminForms/Add" VARNAME="_XLAT_"/>
			<A onClick='document.forms[0].submit();' HREF="#" onmouseover="window.status='Variables._XLAT_';return true;" onmouseout="window.status='';return true;" REPLACEALL="Variables._XLAT_"><CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/TextButton"><ARGUMENT NAME="buttonkey" VALUE="dvin/UI/CSAdminForms/Add"/></CALLELEMENT></A> 
        </TD>
    </TR>
</TABLE>
</SATELLITE.FORM>
</FTCS>