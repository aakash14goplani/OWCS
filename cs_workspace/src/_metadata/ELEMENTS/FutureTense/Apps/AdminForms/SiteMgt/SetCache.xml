<?XML VERSION="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.0">

	<!-- ####################################################################### -->
	<!-- SiteMgt/SetCache                                                        -->
	<!-- FutureTense, Inc.                                                       -->
	<!-- ####################################################################### -->

	<IF COND="IsVariable.key=true">
		<THEN>
			<IF COND="Variables.key!=%">
				<THEN>
					<SETSSVAR NAME="sitekey" VALUE="Variables.key"/>
				</THEN>
			</IF>
		</THEN>
	</IF>
	<SETVAR NAME="tablename" VALUE="SiteCatalog"/>
	<SETVAR NAME="pagename" VALUE="Variables.key"/>
	<SELECTTO FROM="Variables.tablename" WHAT="*" LIST="ResultsList" WHERE="pagename" />
	<table class="width-outer-70" cellspacing="0" cellpadding="0" border="0">
		<tr><td>
			<span class="title-text"><XLAT.STREAM KEY="dvin/AdminForms/ModifyPageCache"/></span>
		</td></tr>
		<callelement NAME="OpenMarket/Xcelerate/UIFramework/Util/TitleBar"/>
	</table>    
	<IF COND="ResultsList.#numRows=0">
		<THEN>
		<div class="width-outer-70">
			<XLAT.LOOKUP KEY="dvin/AdminForms/NoPageMatch" VARNAME="nopagematch"/>
			<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/ShowMessage">
				<ARGUMENT NAME="msgtext" VALUE="Variables.nopagematch"/>
				<ARGUMENT NAME="severity" VALUE="error"/>
			</CALLELEMENT>
			<br/><br/>
			<XLAT.LOOKUP KEY="dvin/UI/Back" VARNAME="_XLAT_" ENCODE="false" ESCAPE="true"/>
			<SATELLITE.FORM ASSEMBLER="query">
                <STRING.ENCODE VARIABLE="charset" VARNAME="OutputQString"/>
				<INPUT TYPE="HIDDEN" NAME="_charset_" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>
				<STRING.ENCODE VARIABLE="cs_environment" VARNAME="OutputQString"/>
				<INPUT TYPE="hidden"  NAME="cs_environment" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>
				<STRING.ENCODE VARIABLE="cs_formmode" VARNAME="OutputQString"/>
				<INPUT TYPE="hidden"  NAME="cs_formmode" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>
				<A onClick="javascript:history.go(-1);return false;" HREF="#" onmouseover="window.status='Variables._XLAT_';return true;" onmouseout="window.status='';return true;" REPLACEALL="Variables._XLAT_"><CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/TextButton"><ARGUMENT NAME="buttonkey" VALUE="dvin/UI/Back"/></CALLELEMENT></A>
			</SATELLITE.FORM>
		</div>
		</THEN>
		<ELSE>
			<SATELLITE.FORM ASSEMBLER="query" METHOD="post">
				<STRING.ENCODE VARIABLE="charset" VARNAME="OutputQString"/>
				<INPUT TYPE="HIDDEN" NAME="_charset_" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>
				<STRING.ENCODE VARIABLE="cs_environment" VARNAME="OutputQString"/>
				<INPUT TYPE="hidden"  NAME="cs_environment" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>
				<STRING.ENCODE VARIABLE="cs_formmode" VARNAME="OutputQString"/>
				<INPUT TYPE="hidden"  NAME="cs_formmode" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>
                <INPUT TYPE="HIDDEN" NAME="pagename" VALUE="FutureTense/Apps/AdminForms/SiteMgt"/>
				            
				<TABLE class="width-outer-50" BGCOLOR="#FFFFFF" BORDER="0"  CELLPADDING="0" CELLSPACING="0">
					
					<CALLELEMENT NAME="FutureTense/Apps/AdminForms/Common/CacheInfo"/>
						
					<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
					<TR>
						<TD class="form-label-inset"  NOWRAP="true" COLSPAN="2">
							<XLAT.STREAM KEY="dvin/AdminForms/Checkpagmodify"/>
						</TD>
						<TD>
							<XLAT.LOOKUP KEY="dvin/Common/All" VARNAME="_XLAT_"/>
							<A onClick="selectAll(document.forms[0],true,'FTcheckbox');" HREF="#" onmouseover="window.status='Variables._XLAT_';return true;" onmouseout="window.status='';return true;" REPLACEALL="Variables._XLAT_"><CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/TextButton"><ARGUMENT NAME="buttonkey" VALUE="dvin/Common/All"/></CALLELEMENT></A>
							<XLAT.LOOKUP KEY="dvin/Common/None" VARNAME="_XLAT_"/>
							<A onClick="selectAll(document.forms[0],false,'FTcheckbox');" HREF="#" onmouseover="window.status='Variables._XLAT_';return true;" onmouseout="window.status='';return true;" REPLACEALL="Variables._XLAT_"><CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/TextButton"><ARGUMENT NAME="buttonkey" VALUE="dvin/Common/None"/></CALLELEMENT></A>
						</TD>
					</TR>
					
					<tr><td colspan="3">
					<TABLE width="100%" BORDER="0" CELLSPACING="0" CELLPADDING="0">
					<tr>
						<td></td>
						<td class="tile-dark" HEIGHT="1">
							<IMG WIDTH="1" HEIGHT="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" 
							REPLACEALL="Variables.cs_imagedir"/> 
						</td>
						<td></td>
					</tr>
					<tr>
						<td class="tile-dark" VALIGN="top" WIDTH="1"><BR/></td>
						<td> 
							<TABLE width="100%" cellpadding="0" cellspacing="0" border="0" bgcolor="#ffffff">
								<tr>
									<td colspan="7" class="tile-highlight"><IMG WIDTH="1" HEIGHT="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/>
									</td>
								</tr>
								<tr>
									<td class="tile-a" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;&nbsp;&nbsp;&nbsp;</td>
									<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir"><DIV class="new-table-title"><XLAT.STREAM KEY="dvin/AdminForms/PageName"/></DIV></td>
									<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;&nbsp;&nbsp;&nbsp;</td>
									<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir"><DIV class="new-table-title"><XLAT.STREAM KEY="dvin/AdminForms/CacheInfo"/></DIV></td>
									<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;&nbsp;&nbsp;&nbsp;</td>
									<td class="tile-b" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir"><DIV class="new-table-title"><XLAT.STREAM KEY="dvin/AdminForms/ApplyQ"/></DIV></td>
									<td class="tile-c" background="Variables.cs_imagedir/graphics/common/screen/grad.gif" REPLACEALL="Variables.cs_imagedir">&nbsp;&nbsp;&nbsp;&nbsp;</td>
								</tr>
								<setvar NAME="rowStyle" VALUE="tile-row-normal"/>
								<SETVAR NAME="separatorLine" VALUE="0"/>
								<SETCOUNTER NAME="pgcount"  VALUE="0"/>
								<LOOP LIST="ResultsList">
									<SETVAR NAME="separatorLine" VALUE="1"/>
									<TR class="Variables.rowStyle"  REPLACEALL="Variables.rowStyle"><td><BR /></td>
										<TD class="small-text-inset" style="white-space: normal;" NOWRAP="NOWRAP" VALIGN="BASELINE">
											<INPUT TYPE="HIDDEN" NAME="pagenameCounters.pgcount" VALUE="ResultsList.pagename" REPLACEALL="Counters.pgcount,ResultsList.pagename"/>
											<INPUT TYPE="HIDDEN" NAME="rootelement" VALUE="ResultsList.rootelement" REPLACEALL="ResultsList.rootelement"/>
											<INPUT TYPE="HIDDEN" NAME="csstatus" VALUE="ResultsList.csstatus" REPLACEALL="ResultsList.csstatus"/>
											<INPUT TYPE="HIDDEN" NAME="resargs1" VALUE="ResultsList.resargs1" REPLACEALL="ResultsList.resargs1"/>
											<INPUT TYPE="HIDDEN" NAME="resargs2" VALUE="ResultsList.resargs2" REPLACEALL="ResultsList.resargs2"/>
											<INPUT TYPE="HIDDEN" NAME="acl" VALUE="ResultsList.acl" REPLACEALL=""/>
											<STRING.STREAM VALUE="ResultsList.pagename"/>
										</TD>
										<td><BR /></td>
										<TD class="small-text-inset" style="white-space: normal;" VALIGN="BASELINE" NOWRAP="NOWRAP">
											<STRING.STREAM VALUE="ResultsList.cscacheinfo"/>
											<INPUT TYPE="HIDDEN" NAME="cacheinfo" VALUE=""/>
										</TD>
										<td><BR /></td>
										<TD class="small-text-inset" style="vertical-align: middle;" NOWRAP="NOWRAP" VALIGN="BASELINE">
											<INPUT TYPE="checkbox" NAME="FTcheckbox"/>
										</TD>
										<td><BR/></td>
									</TR>
									<INCCOUNTER NAME="pgcount" VALUE="1"/>
									<IF COND="Variables.rowStyle=tile-row-normal">
									<THEN>
										<SETVAR NAME="rowStyle" VALUE="tile-row-highlight"/>
									</THEN>
									<ELSE>
										<SETVAR NAME="rowStyle" VALUE="tile-row-normal"/>
									</ELSE>
									</IF>
								</LOOP>
							</TABLE>
							</td><td class="tile-dark" VALIGN="top" WIDTH="1"><BR /></td>
						</tr>
						<tr>
							<td colspan="3" class="tile-dark" VALIGN="TOP" HEIGHT="1"><IMG WIDTH="1" HEIGHT="1" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/></td>
						</tr>
						<tr>
							<td></td><td background="Variables.cs_imagedir/graphics/common/screen/shadow.gif" REPLACEALL="Variables.cs_imagedir"><IMG WIDTH="1" HEIGHT="5" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/></td><td></td>
						</tr>
					</TABLE> 
					</td></tr>
					
					<TR>
						<TD COLSPAN="3" HEIGHT="5"></TD>
					</TR>
					<TR>
						<TD ALIGN="left" VALIGN="TOP">
							<INPUT TYPE="hidden" NAME="ftcmd" VALUE="replacerows"/>
							<INPUT TYPE="hidden" NAME="tablename" VALUE="SiteCatalog"/>
							<INPUT TYPE="HIDDEN" NAME="Operation" VALUE="ModifyPageCache"/>
							<INPUT TYPE="HIDDEN" NAME="rollupvariables" VALUE="true"/>
							<XLAT.LOOKUP KEY="dvin/AdminForms/Apply" VARNAME="_XLAT_"/>
							<A onClick="setupModifyCacheParams(document.forms[0]); document.forms[0].submit();" HREF="#" onmouseover="window.status='Variables._XLAT_';return true;" onmouseout="window.status='';return true;" REPLACEALL="Variables._XLAT_"><CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/TextButton"><ARGUMENT NAME="buttonkey" VALUE="dvin/AdminForms/Apply"/></CALLELEMENT></A>
						</TD>
					</TR>
				</TABLE>
			</SATELLITE.FORM>
		</ELSE>
	</IF>
</FTCS>
