<?XML VERSION="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.0">

<!-- ####################################################################### -->
<!-- SiteMgt/Modify                                                          -->
<!-- FutureTense, Inc.                                                       -->
<!-- ####################################################################### -->

<SATELLITE.FORM ASSEMBLER="query" method="POST">
    <STRING.ENCODE VARIABLE="charset" VARNAME="OutputQString"/>
	<INPUT TYPE="HIDDEN" NAME="_charset_" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>
	<STRING.ENCODE VARIABLE="cs_environment" VARNAME="OutputQString"/>
	<INPUT TYPE="hidden"  NAME="cs_environment" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>
	<STRING.ENCODE VARIABLE="cs_formmode" VARNAME="OutputQString"/>
	<INPUT TYPE="hidden"  NAME="cs_formmode" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>

<INPUT TYPE="HIDDEN" NAME="pagename" VALUE="FutureTense/Apps/AdminForms/SiteMgt"/>
<INPUT TYPE="HIDDEN" NAME="testpagename" VALUE="Variables.key" REPLACEALL="Variables.key"/>
<SETVAR NAME="pagename" VALUE="Variables.key"/>
<SELECTTO FROM="SiteCatalog" WHAT="*" WHERE="pagename" LIST="ResultsList"/>

<table class="width-outer-70" cellspacing="0" cellpadding="0" border="0">
	<tr><td>
		<span class="title-text"><XLAT.STREAM KEY="dvin/AdminForms/ModifyPage"/></span>
	</td></tr>
	<callelement NAME="OpenMarket/Xcelerate/UIFramework/Util/TitleBar"/>
</table>

<TABLE class="width-outer-50" BGCOLOR="#FFFFFF" BORDER="0" WIDTH="400" CELLPADDING="0" CELLSPACING="0">
    <CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
    <TR>
        <TD class="form-label-text" align="right" NOWRAP="true">
  			<XLAT.STREAM KEY="dvin/AdminForms/PageName"/>:
        </TD>
        <TD class="form-inset" align="left">
			<STRING.ENCODE LIST="ResultsList" COLUMN="pagename" VARNAME="encpagename"/>
  			<REPLACE STR="Variables.encpagename">
    				 Variables.encpagename
       				<INPUT TYPE="hidden" NAME="NewPagename" VALUE="Variables.encpagename"/>
   			</REPLACE>
        </TD>
    </TR>
    <CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
	<TR>
        <TD class="form-label-text" align="right" NOWRAP="true">
  			<XLAT.STREAM KEY="dvin/AdminForms/RootElement"/>:
		</TD>
        <TD class="form-inset" align="left">
   			<REPLACE STR="ResultsList.rootelement">
        			<INPUT TYPE="text" NAME="rootelement" SIZE="32" VALUE="ResultsList.rootelement"/>
       		</REPLACE>
        </TD>
    </TR>
    <CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
	<TR>
        <TD class="form-label-text"  align="right" NOWRAP="true">
  			<XLAT.STREAM KEY="dvin/AT/Common/Status"/>:
        </TD>
        <TD class="form-inset" align="left">
    		<SELECT NAME="csstatus">
				<SCRIPT LANGUAGE="JavaScript">
					<REPLACEALL LIST="ResultsList.csstatus, Variables._XLAT_">
					<XLAT.LOOKUP KEY="dvin/AdminForms/Live" VARNAME="_XLAT_" ENCODE="false" ESCAPE="true"/>
					document.writeln(<![CDATA['<OPTION VALUE="Live" ' + shouldSelectNoCase("live","ResultsList.csstatus") + ' > ]]>Variables._XLAT_');
					<XLAT.LOOKUP KEY="dvin/AdminForms/Pending" VARNAME="_XLAT_" ENCODE="false" ESCAPE="true"/>
							document.writeln(<![CDATA['<OPTION VALUE="Pending" ' + shouldSelectNoCase("pending","ResultsList.csstatus") + ' > ]]>Variables._XLAT_');
					</REPLACEALL>
				</SCRIPT>
    		</SELECT>
        </TD>
    </TR>
    <CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
	<TR>
        <TD class="form-label-text"  align="right" NOWRAP="true">
  			<XLAT.STREAM KEY="dvin/AdminForms/Arguments1"/>:
        </TD>
        <TD class="form-inset" align="left">
      		<REPLACE STR="ResultsList.resargs1">
                <INPUT TYPE="text" NAME="resargs1" SIZE="32" VALUE="ResultsList.resargs1"/>
			</REPLACE>
       </TD>
    </TR>
	<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
    <TR>
        <TD class="form-label-text"  align="right" NOWRAP="true">
            <XLAT.STREAM KEY="dvin/AdminForms/Arguments2"/>:
        </TD>
        <TD class="form-inset" align="left">
     		<REPLACE STR="ResultsList.resargs2">
                    <INPUT TYPE="text" NAME="resargs2" SIZE="32" VALUE="ResultsList.resargs2"/>
            </REPLACE>
        </TD>
    </TR>
	<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
    <TR>
        <TD class="form-label-text" align="right" VALIGN="TOP" NOWRAP="true">
            <XLAT.STREAM KEY="dvin/AdminForms/ACL"/>:
        </TD>
        <TD class="form-inset" align="left">
 			<INPUT TYPE="HIDDEN" NAME="acl" VALUE=""/>
            <CALLELEMENT NAME="FutureTense/Apps/AdminForms/Common/ACLList">
                    <ARGUMENT NAME="selectList" VALUE="ResultsList.acl"/>
            </CALLELEMENT>
        </TD>
    </TR>
	
	<CALLELEMENT NAME="FutureTense/Apps/AdminForms/Common/CacheInfo">
		<ARGUMENT NAME="cacheinfo" VALUE="ResultsList.cacheinfo"/>
	</CALLELEMENT>
    	
    <callelement NAME="OpenMarket/Xcelerate/UIFramework/Util/FooterBar"/>
	<TR>
        <TD></TD><TD>
			<INPUT TYPE="hidden" NAME="tablename" VALUE="SiteCatalog"/>
			<INPUT TYPE="hidden" NAME="ftcmd" VALUE="replacerow"/>
			<INPUT TYPE="HIDDEN" NAME="Operation" VALUE="ModifyPage"/>
			<INPUT TYPE="HIDDEN" NAME="rollupvariables" VALUE="false"/>
			<XLAT.LOOKUP KEY="dvin/AdminForms/Modify" VARNAME="_XLAT_"/>
    		<A onClick="checkModifyPage(document.forms[0]); document.forms[0].submit();" HREF="#" onmouseover="window.status='Variables._XLAT_';return true;" onmouseout="window.status='';return true;" REPLACEALL="Variables._XLAT_"><CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/TextButton"><ARGUMENT NAME="buttonkey" VALUE="dvin/AdminForms/Modify"/></CALLELEMENT></A>
        </TD>
    </TR>
</TABLE>
</SATELLITE.FORM>
</FTCS>
