<?XML VERSION="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.0">

<!-- ####################################################################### -->
<!-- SiteMgt/Add                                                             -->
<!-- FutureTense, Inc.                                                       -->
<!-- ####################################################################### -->

<IF COND="IsVariable.key=true"><THEN>
    <IF COND="Variables.key!=%"><THEN>
        <SETSSVAR NAME="sitekey" VALUE="Variables.key"/>
    </THEN></IF>
</THEN><ELSE>
    <SETVAR NAME="key" VALUE="Variables.empty"/>
</ELSE></IF>

<SATELLITE.FORM ASSEMBLER="query" METHOD="POST" onSubmit="return checkAddPage(this)">
	<STRING.ENCODE VARIABLE="charset" VARNAME="OutputQString"/>
	<INPUT TYPE="HIDDEN" NAME="_charset_" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>
	<STRING.ENCODE VARIABLE="cs_environment" VARNAME="OutputQString"/>
	<INPUT TYPE="hidden"  NAME="cs_environment" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>
	<STRING.ENCODE VARIABLE="cs_formmode" VARNAME="OutputQString"/>
	<INPUT TYPE="hidden"  NAME="cs_formmode" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>   

<INPUT TYPE="HIDDEN" NAME="pagename" VALUE="FutureTense/Apps/AdminForms/SiteMgt"/>

<table class="width-outer-70" cellspacing="0" cellpadding="0" border="0">
	<tr><td>
		<span class="title-text"><XLAT.STREAM KEY="dvin/AdminForms/AddPage"/></span>
	</td></tr>
	<callelement NAME="OpenMarket/Xcelerate/UIFramework/Util/TitleBar"/>
</table>

<TABLE  class="width-outer-50" BGCOLOR="#FFFFFF" BORDER="0" CELLPADDING="0" CELLSPACING="0">
    <CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
	<TR>
        <TD class="form-label-text" align="right" NOWRAP="true">
            <span class="alert-color">*</span><XLAT.STREAM KEY="dvin/AdminForms/PageName"/>:
        </TD>
        <TD class="form-inset" align="left">
			<STRING.ENCODE VARIABLE="key" VARNAME="OutputQString"/>
			<INPUT TYPE="text" NAME="NewPagename" SIZE="32" VALUE="Variables.OutputQString" REPLACEALL="Variables.OutputQString"/>                 
    	</TD>
    	<TD></TD>
    </TR>
    <CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
	<TR>
        <TD class="form-label-text" align="right" NOWRAP="true">
   			<span class="alert-color">*</span><XLAT.STREAM KEY="dvin/AdminForms/RootElement"/>:
		</TD>
        <TD class="form-inset" align="left">
  			<INPUT TYPE="text" NAME="rootelement" SIZE="32" VALUE=""/>
        </TD>
    	<TD></TD>
    </TR>
    <CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
	<TR>
        <TD class="form-label-text" align="right" NOWRAP="true">
   			<XLAT.STREAM KEY="dvin/AT/Common/Status"/>: 
        </TD>
        <TD class="form-inset" align="left">
			<SELECT NAME="csstatus">
				<OPTION VALUE="Live"/><XLAT.STREAM KEY="dvin/AdminForms/Live"/> 
				<OPTION VALUE="Pending" SELECTED="true"/><XLAT.STREAM KEY="dvin/AdminForms/Pending"/> 
			</SELECT>
        </TD>
    	<TD></TD>
    </TR>
    <CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
	<TR>
        <TD class="form-label-text" align="right" NOWRAP="true">
            <FONT FACE="Arial, Helvetica" SIZE="-1">
    			<XLAT.STREAM KEY="dvin/AdminForms/Arguments1"/>:
            </FONT>
        </TD>
        <TD class="form-inset" align="left">
            <INPUT TYPE="text" NAME="resargs1" SIZE="32" VALUE=""/>
        </TD>
    	<TD></TD>
    </TR>
	<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
    <TR>
        <TD class="form-label-text" align="right" NOWRAP="true">
   			<XLAT.STREAM KEY="dvin/AdminForms/Arguments2"/>:
        </TD>
        <TD class="form-inset" align="left">
            <INPUT TYPE="text" NAME="resargs2" SIZE="32" VALUE=""/>
        </TD>
    	<TD></TD>
    </TR>
	<CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/RowSpacer"/>
    <TR>
        <TD class="form-label-text" align="right" VALIGN="TOP" NOWRAP="true">
            <XLAT.STREAM KEY="dvin/AdminForms/ACL"/>:
        </TD>
        <TD class="form-inset" align="left">
    		<INPUT TYPE="HIDDEN" NAME="acl" VALUE=""/>
            <CALLELEMENT NAME="FutureTense/Apps/AdminForms/Common/ACLList"/>
        </TD>
    	<TD></TD>
    </TR>
    <CALLELEMENT NAME="FutureTense/Apps/AdminForms/Common/CacheInfo"/>
    <callelement NAME="OpenMarket/Xcelerate/UIFramework/Util/FooterBar"/>
    <TR>
        <TD></TD><TD>
   			<INPUT TYPE="hidden" NAME="tablename" VALUE="SiteCatalog"/>
			<INPUT TYPE="hidden" NAME="ftcmd" VALUE="addrow"/>
			<INPUT TYPE="HIDDEN" NAME="Operation" VALUE="AddPage"/>
			<INPUT TYPE="HIDDEN" NAME="rollupvariables" VALUE="false"/>
			<XLAT.LOOKUP KEY="dvin/UI/CSAdminForms/Add" VARNAME="_XLAT_"/>
			<A onClick='if(checkAddPage(document.forms[0]))document.forms[0].submit();' HREF="#" onmouseover="window.status='Variables._XLAT_';return true;" onmouseout="window.status='';return true;" REPLACEALL="Variables._XLAT_"><CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/TextButton"><ARGUMENT NAME="buttonkey" VALUE="dvin/UI/CSAdminForms/Add"/></CALLELEMENT></A>
        </TD>
    </TR>
</TABLE>
</SATELLITE.FORM>
</FTCS>
