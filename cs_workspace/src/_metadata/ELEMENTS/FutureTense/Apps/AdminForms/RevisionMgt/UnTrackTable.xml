<?XML VERSION="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.0">
<div class="width-outer-70">
<SETCOUNTER NAME="UnTrackCounter" VALUE="1"/>
<SETCOUNTER NAME="EndCount" VALUE="Variables.numtables"/>
<INCCOUNTER NAME="EndCount" VALUE="1"/>
<LOOP UNTIL="Counters.UnTrackCounter=Counters.EndCount">
    <SETVAR NAME="shouldUnTrack" VALUE="Variables.dountrackCounters.UnTrackCounter"/>
    <IF COND="Variables.shouldUnTrack=true"><THEN>
        <!-- clear out errno -->
        <SETVAR NAME="errno" VALUE="0"/>

        <!-- do the untrack -->
        <SETVAR NAME="untrackTable" VALUE="Variables.untrackmeCounters.UnTrackCounter"/>
        <UNTRACKTABLE TABLE="Variables.untrackTable"/>

        <!-- See if untrack succeeded -->
        <IF COND="Variables.errno=0">
             <THEN>
                  <!-- if succeeded, brag about it -->
                  
                  <XLAT.LOOKUP KEY="dvin/AdminForms/UnTracktable" VARNAME="untracktable"/>
				  <CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/ShowMessage">
						<ARGUMENT NAME="msgtext" VALUE="Variables.untracktable"/>
						<ARGUMENT NAME="severity" VALUE="info"/>
				  </CALLELEMENT>
             </THEN>
             <ELSE>
                  <!-- otherwise, call an error reporting element -->
                  <XLAT.LOOKUP KEY="dvin/AdminForms/UnTracktableFailed" VARNAME="untrackfailed"/>
				  <CALLELEMENT NAME="OpenMarket/Xcelerate/UIFramework/Util/ShowMessage">
						<ARGUMENT NAME="msgtext" VALUE="Variables.untrackfailed"/>
						<ARGUMENT NAME="severity" VALUE="error"/>
				  </CALLELEMENT>
                  <CALLELEMENT NAME="FutureTense/Apps/AdminForms/RevisionMgt/errors"/><BR/><BR/>
             </ELSE>
        </IF>
    </THEN></IF>
    <INCCOUNTER NAME="UnTrackCounter" VALUE="1"/>
</LOOP>
</div>
</FTCS>
