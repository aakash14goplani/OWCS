<?xml version="1.0" ?>
<!DOCTYPE ftcs SYSTEM "futuretense_cs.dtd">
<ftcs version="1.2">
<!-- AlloyUI/Asset/Util/callAssetMaker
-
- INPUT
-	AssetType
-
- OUTPUT
-	Properties of the assettype as ics variables
-
This template uses <CALLJAVA> tag to call AssetMaker to get all of the
properties associated with the assettype from the descriptor file.

-->
<CALLJAVA CLASS="com.openmarket.assetmaker.asset.AssetMaker" TYPE="1">
    <ARGUMENT NAME="command" VALUE="parseproperties"/>
    <ARGUMENT NAME="propertytags" VALUE="storage,inputform"/>
    <ARGUMENT NAME="assettype" VALUE="Variables.AssetType"/>
</CALLJAVA>
<CALLJAVA CLASS="com.openmarket.assetmaker.asset.AssetMaker">
   <ARGUMENT NAME="command" VALUE="parseasset"/>
	<ARGUMENT NAME="assettype" VALUE="Variables.AssetType"/>
</CALLJAVA>
</ftcs>